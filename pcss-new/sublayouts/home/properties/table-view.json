                        "props": {
                        },
                        "children": [
                          {
                            "component": "Box",
                            "props": {
                             
                            },
                            "children": [
                              {
                                "component": "Icon",
                                "props": {
                                  "name": "",
                                  "color": "black"
                                }
                              }
                            ]
                          },
                          {
                            "component": "Text",
                            "props": {
                              "text": "View",
                              "size": "sm",
                              "color": "black"
                            }
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "component": "EventButton",
                    "props": {
                      "height": 25,
                      "padding": 5,
                      "size": "xs",
                      "eventType": "BTN_CLICK",
                      "buttonCode": "BTN_VIEW_BEG",
                      "value": {
                        "itemCode": "{{code}}",
                        "hint": "BTN_VIEW_BEG"
                      }
                    },
                    "children": [
                      {
                        "component": "Box",
                        "props": {
                        },
                        "children": [
                          {
                            "component": "Box",
                            "props": {
                             
                            },
                            "children": [
                              {
                                "component": "Icon",
                                "props": {
                                  "name": "edit",
                                  "color": "green"
                                }
                              }
                            ]
                          },
                          {
                            "component": "Text",
                            "props": {
                              "text": "",
                              "size": "sm",
                              "color": "white"
                            }
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "component": "EventButton",
                    "props": {
                      "height": 25,
                      "padding": 5,
                      "size": "xs",
                      "eventType": "BTN_CLICK",
                      "buttonCode": "BTN_VIEW_BEG",
                      "value": {
                        "itemCode": "{{code}}",
                        "hint": "BTN_VIEW_BEG"
