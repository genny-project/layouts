{
  "Grid": {
    "position": [0, 0],
    "rows": 1,
    "cols": [
      "40px",
      1,
      {
        "style": {
          "flex": "0 0 10px",
          "display": "flex",
          "flexDirection": "column",
          "alignItems": "flex-end"
        }
      }
    ],
    "children": [
      {
        "ImageView": {
          "position": [0, 0],
          "style": {
            "margin": "0px",
            "maxWidth": "30px",
            "width": "30px"
          },
          "src": "BE.link.APPLICANT.PRI_IMAGE_URL"
        }
      },
      {
        "Grid": {
          "position": [0, 1],
          "rows": 2,
          "cols": 1,
          "children": [
            {
              "Selector": {
                "position": [1, 0],
                "checkValues": "BE.link.APPLICANT.PRI_STUDENT_ID",
                "showIfNotNull": true,
                "children": [
                  {
                    "p": {
                      "position": [0, 0],
                      "style": {
                        "margin": "0px"
                      },
                      "children": "IDs: ${BE.link.APPLICANT.PRI_STUDENT_ID}"
                    }
                  }
                ]
              }
            },
            {
              "p": {
                "position": [1, 0],
                "style": {
                  "margin": "0px"
                },
                "children": "BE.PRI_APPLICANT_FULLNAME"
              }
            },
            {
              "p": {
                "position": [1, 0],
                "style": {
                  "margin": "0px"
                },
                "children": [
                  {
                    "SublayoutLoader": {
                      "layoutCode": "card-edu-provider-name",
                      "style": {},
                      "aliases": {
                        "BE": "BE.PRI_EDU_PROVIDER_CODE",
                        "GROUP": "ROOT"
                      }
                    }
                  }
                ]
              }
            },
            {
              "p": {
                "position": [1, 0],
                "style": {
                  "margin": "0px"
                },
                "children": [
                  {
                    "SublayoutLoader": {
                      "layoutCode": "card-internship-name",
                      "style": {},
                      "aliases": {
                        "BE": "BE.PRI_BEG_CODE",
                        "GROUP": "ROOT"
                      }
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "GennyDropdown": {
          "position": [0, 2],
          "inline": false,
          "isSlide": false,
          "animateHeader": false,
          "animateFooter": true,
          "stopPropagationOnClick": true,
          "style": {
            "paddingBottom": "2px",
            "margin": "-5px -5px 0"
          },
          "contentStyle": {
            "backgroundColor": "#fff",
            "boxShadow": "0px 2px 10px 5px rgba(0, 0, 0, 0.25)",
            "display": "flex",
            "flexDirection": "column",
            "width": "120px",
            "left": "100%",
            "padding": "5px 5px 0",
            "transform": "translate(-100%)",
            "top": "20px"
          },
          "tagStyle": {
            "display": "none"
          },
          "header": [
            {
              "IconSmall": {
                "name": "more_horiz",
                "style": {
                  "padding": "0"
                }
              }
            }
          ],
          "children": [
            {
              "SublayoutLoader": {
                "layoutCode": "card-button-actions",
                "aliases": {
                  "BE": "BE.code",
                  "GROUP": "ROOT"
                }
              }
            }
          ]
        }
      },
      {
        "GennyStatus": {
          "position": [0, 2],
          "root": "BE.code",
          "style": {
            "height": "25px",
            "width": "10px"
          },
          "statusStyle": {
            "height": "100%",
            "width": "10px",
            "maxHeight": "40px"
          }
        }
      }
    ]
  }
}
