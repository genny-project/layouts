{
  "layout": [
    {
      "AppHolder": {
        "cols": [0,5],
        "rows": 1,
        "children": [
          {
            "Sidebar": {
              "position": [0,0],
              "style": {
                "backgroundColor": "PROJECT.PRI_COLOR",
                "color": "white",
                "textAlign": "center"
              },
              "hasImage": true,
              "src": "PROJECT.PRI_LOGO",
              "children": [
                {
                  "GennyTreeView": {
                    "position": [1,0],
                    "style": {
                    "marginTop": "50px"
                  },
                    "root": "GRP_ROOT"
                  }
                }
              ]
            }
          },
          {
            "Grid": {
              "cols": [{"style": { "flexGrow" : 1, "width" : "100%"}}],
              "rows": ["90px", { "style" : {"flex": "1 1 auto", "overflow": "hidden" }}, "30px"],
              "position": [0,1],
              "className": "app-main",
              "children": [
                {
                  "GennyHeader": {
                    "position": [0,0],
                    "hideSubheader": true,
                    "style": {
                      "backgroundColor": "PROJECT.PRI_COLOR",
                      "color": "white"
                    },
                    "projectTitle" : "PROJECT.PRI_NAME",
                    "projectGreeting" : "PROJECT.PRI_GREETING",
                    "userName" : "USER.PRI_FIRSTNAME",
                    "userImage" : "USER.PRI_IMAGE_URL"
                  }
                },
                {
                  "Selector": {
                    "position": [
                      0,
                      0
                    ],
                    "checkValues": [
                      "USER.PRI_IS_SELLER"
                    ],
                    "showValues": [
                      true
                    ],
                    "children": [
                      {
                        "div": {
                          "style": {
                            "width": "100%",
                            "height": "70px",
                            "background": "PROJECT.PRI_COLOR"
                          },
                          "children": [
                            {
                              "GennyButton": {
                                "buttonCode": "BTN_ADD_BEG",
                                "style": {
                                  "padding": "10px",
                                  "width": "250px"
                                },
                                "buttonStyle": {
                                  "background": "PROJECT.PRI_COLOR",
                                  "border": "1px solid white"
                                },
                                "value": {},
                                "children": "+ NEW INTERNSHIP"
                              }
                            }  
                          ]
                        }
                      }
                    ]
                  }
                },
                {
                  "AppContent": {
                    "position": [1,0],
                    "children" : [
                      {
                        "div": {
                          "children": ""
                        }
                      }
                    ]
                  }
                },
                {
                  "Footer": {
                    "position": [2,0],
                    "style": {
                      "backgroundColor": "PROJECT.PRI_COLOR",
                      "color": "white"
                    },
                    "version" : "PROJECT.PRI_VERSION",
                    "poweredBy" : {
                      "caption" : "PROJECT.PRI_POWERED_BY"
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
