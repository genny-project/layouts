{
  "layout": {
    "title": "Interns",
    "appColor": "white",
    "backgroundColor": "whitesmoke",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    },
    "sidebarRight": {
      "variant": "default",
      "onlyShowIf": {
        "user.attributes.PRI_IS_AGENT.value": {
          "$eq": true
        }
      }
    }
  },
  "children": [
    {
      "component": "Sublayout",
      "props": {
        "layoutName": "tabs/tabs-default",
        "title": "Internships",
        "tabs": [
          { "title": "Table View", "key": 0 },
          { "title": "Applications", "key": 1 },
          { "title": "Details View", "key": 2 }
        ],
        "content": [
          {
            "component": "Sublayout",
            "props": {
              "layoutName": "tables/table-interns"
            }
          },
          {
            "component": "Sublayout",
            "props": {
              "layoutName": "home/agent/bucket"
            }
          },
          {
            "component": "Sublayout",
            "props": {
              "layoutName": "home/agent/internship-details"
            }
          }
        ]
      }
    }
  ]
}