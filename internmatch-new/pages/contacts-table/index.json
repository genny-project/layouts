{
  "layout": {
    "title": "All Contacts",
    "appColor": "white",
    "backgroundColor": "whitesmoke",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    },
    "sidebarRight": {
      "variant": "default",
      "onlyShowIf": {
        "$or": [
          {
            "user.attributes.PRI_IS_AGENT.value": {
              "$eq": true
            }
          },
          {
            "user.attributes.PRI_IS_EDU_PROVIDER_STAFF.value": {
              "$eq": true
            }
          },
          {
            "user.attributes.PRI_IS_BUYER.value": {
              "$eq": true
            }
          }
        ]
      }
    }
  },
  "children": [{
    "component": "Box",
    "children": [{
      "component": "Table",
      "props": {
        "code": "GRP_CONTACTS",
        "renderWrapper": {
          "component": "EventButton",
          "props": {
            "height": "20px",
            "fontWeight": "300",
            "fontSize": 10,
            "eventType": "BTN_CLICK",
            "buttonCode": "BTN_TABLE_SELECT",
            "value": {
              "itemCode": "_celld.attributeCode",
              "userCode": "{{user.data.code}}"
            }
          }
        },
        "columns": [{
            "Header": "Actions",
            "accessor": "actions",
            "renderButton": [
            ]
          },
          {
            "Header": "First Name",
            "accessor": "firstName",
            "id": "firstName",
            "key": "firstName",
            "filterType": "string"
          },
          {
            "Header": "Last Name",
            "accessor": "lastName",
            "filterType": "string"
          },
          {
            "Header": "Email",
            "accessor": "email",
            "filterType": "string"
          }, {
            "Header": "Contact Number",
            "accessor": "contactNumber",
            "filterType": "string"
          }
        ],
        "tableBackgroundColor": "#cccccc",
        "data": "_query.tableData",
        "tableHeight": "500px",
        "tableWidth": "100%",
        "itemsPerPage": 10
      }
    }]
  }],
  "query": [{
      "operator": "getBE",
      "id": "GRP_RESULTS",
      "as": "contacts"
    },
    {
      "operator": "scope",
      "path": "contacts.links",
      "as": "contacts",
      "scope": {
        "operator": "getBE",
        "basePath": "baseEntities.data",
        "id": "{{link.targetCode}}"
      }
    },
    {
      "operator": "scope",
      "path": "contacts",
      "scope": {
        "operator": "populateAttributes",
        "code": "{{code}}",
        "as": "attributes"
      }
    },
    {
      "operator": "scope",
      "path": "contacts",
      "as": "tableData",
      "scope": {
        "operator": "map",
        "fields": {
          "code": "code",
          "firstName": "attributes.PRI_FIRSTNAME.value",
          "lastName": "attributes.PRI_LASTNAME.value",
          "contactNumber": "companyRep.attributes.PRI_MOBILE.value",
          "email": "attributes.PRI_EMAIL.value"
        }
      }
    }
  ]
}