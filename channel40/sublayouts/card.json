{
    "GennyDropdown":{
        "inline":true,
        "isSlide":false,
        "animateHeader":false,
        "animateFooter":true,
        "contentStyle":{
            "backgroundColor":"white",
            "padding":"0"
        },
        "header":[
            {
                "SublayoutLoader": {
                    "layoutCode": "card-header",
                    "aliases": {
                        "GROUP": "ROOT",
                        "BE": "BE.code"
                    }
                }
            }
        ],
        "footer":[
            {
                "SublayoutLoader": {
                    "layoutCode": "card-footer",
                    "aliases": {
                        "GROUP": "ROOT",
                        "BE": "BE.code"
                    }
                }
            }
        ],
        "children":[
            {
                "div":{
                    "style":{
                        "borderRadius":"10px",
                        "padding":"10px",
                        "background":"#ddd"
                    },
                    "children":[
                        {
                            "SublayoutLoader": {
                                "layoutCode": "card-job-detail",
                                "aliases": {
                                    "BE": "BE.code",
                                    "GROUP": "ROOT"
                                }
                            }
                        },
                        {
                            "SublayoutLoader": {
                                "layoutCode": "card-job-lists",
                                "aliases": {
                                    "BE":"BE.code",
                                    "GROUP": "ROOT"
                                }
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues": "ROOT",
                    "showValues": "GRP_APPROVED",
                    "children":[
                        {
                            "ProgressBar":{
                                "style":{
                                    "marginTop":"5px"
                                },
                                "type":2,
                                "progressTotal":100,
                                "progressCurrent":0
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues": "ROOT",
                    "showValues": "GRP_DELIVERED",
                    "children":[
                        {
                            "ProgressBar":{
                                "style":{
                                    "marginTop":"5px"
                                },
                                "type":2,
                                "progressTotal":100,
                                "progressCurrent":100,
                                "progressPercent":"BE.PRI_PROGRESS"
                            }
                        }
                    ]
                }
            }
        ]
    }
}
