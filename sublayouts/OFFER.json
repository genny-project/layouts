{
    "Grid":{
        "rows":[{"style": {"flexGrow": 1, "flexWrap": "wrap"}}],
        "cols":[
            {
                "style":{
                    "flex":"0 0 30px",
                    "display":"flex",
                    "alignItems":"center"
                    
                }
            },
            1,1,1,1,1
        ],
        "className": "bucket-card",
        "children":[
            {
                "ImageView":{
                    "position":[0,0],
                    "style":{
                        "width":"100%",
                        "marginRight":"5px"
                    },
                    "src":"BE.PRI_IMAGE_URL"
                }
            },
            {
                "Selector":{
                    "position":[0,1],
                    "checkValues":"USER.PRI_OWNER",
                    "showValues":"true",
                    "children":[
                        {
                            "span":{
                                "style":{
                                    "fontSize":"0.75em"
                                },
                                "children":"BE.PRI_OFFER_OWNER_PRICE_INC_GST"
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "position":[0,1],
                    "checkValues":"USER.PRI_OWNER",
                    "showValues":"true",
                    "children":[
                        {
                            "span":{
                                "style":{
                                    "fontSize":"0.75em"
                                },
                                "children":"(incl GST)"
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "position":[0,1],
                    "checkValues":"USER.PRI_DRIVER",
                    "showValues":"true",
                    "children":[
                        {
                            "span":{
                                "style":{
                                    "fontSize":"0.75em"
                                },
                                "children":"BE.PRI_OFFER_DRIVER_PRICE_EXC_GST"
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "position":[0,1],
                    "checkValues":"USER.PRI_DRIVER",
                    "showValues":"true",
                    "children":[
                        {
                            "span":{
                                "style":{
                                    "fontSize":"0.75em"
                                },
                                "children":"(ex GST)"
                            }
                        }
                    ]
                }
            },
            {
                "span":{
                    "position":[0,2],
                    "style":{
                        "fontSize":"0.75em"
                    },
                    "children":"BE.links.CREATOR.PRI_FIRSTNAME"
                }
            },
            {
                "span":{
                    "position":[0,2],
                    "style":{
                        "fontSize":"0.75em"
                    },
                    "children":"BE.links.CREATOR.PRI_LASTNAME"
                }
            },
            {
                "RatingDisplay":{
                    "position":[0,2],
                    "fontSize" : "16px",
                    "value":"BE.links.CREATOR.PRI_RATING"
                }
            },
            {
                "Selector":{
                    "checkValues":"USER.PRI_OWNER",
                    "showValues":"true",
                    "position":[0,3],
                    "children":[
                        {
                            "Selector":{
                                "checkValues":"BE.PRI_NEXT_ACTION",
                                "showValues":"OWNER",
                                "children":[
                                    {
                                        "GennyButton":{
                                            "buttonCode":"BTN_CONFIRM_OFFER",
                                            "value":{
                                                "itemCode":"BE.PRI_CODE",
                                                "hint":"ROOT"
                                            },
                                            "style":{
                                                "marginBottom":"5px",
                                                "height":"30px"
                                            },
                                            "type":"confirm",
                                            "children":[
                                                {
                                                    "span":{
                                                        "style":{
                                                            "fontSize":"0.75em",
                                                            "color":"white"
                                                        },
                                                        "children":"ACCEPT"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues":"USER.PRI_OWNER",
                    "showValues":"true",
                    "position":[0,4],
                    "children":[
                        {
                            "Selector":{
                                "checkValues":"BE.PRI_NEXT_ACTION",
                                "showValues":"OWNER",
                                "children":[
                                    {
                                        "GennyButton":{
                                            "buttonCode":"BTN_COUNTER_OFFER",
                                            "value":{
                                                "itemCode":"BE.PRI_CODE",
                                                "hint":"ROOT"
                                            },
                                            "style":{
                                                "marginBottom":"5px",
                                                "height":"30px"
                                            },
                                            "buttonStyle":{
                                                "backgroundColor":"#000"
                                            },
                                            "children":[
                                                {
                                                    "span":{
                                                        "style":{
                                                            "fontSize":"0.75em",
                                                            "color":"white"
                                                        },
                                                        "children":"COUNTER QUOTE"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues":"USER.PRI_OWNER",
                    "showValues":"true",
                    "position":[0,5],
                    "children":[
                        {
                            "GennyButton":{
                                "buttonCode":"BTN_CANCEL_OFFER",
                                "value":{
                                    "itemCode":"BE.PRI_CODE",
                                    "hint":"ROOT"
                                },
                                "style":{
                                    "height":"30px"
                                },
                                "type":"cancel",
                                "children":[
                                    {
                                        "span":{
                                            "style":{
                                                "fontSize":"0.75em",
                                                "color":"white"
                                            },
                                            "children":"HIDE"
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    }
}
