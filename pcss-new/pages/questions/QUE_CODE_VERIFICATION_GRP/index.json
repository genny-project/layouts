{
  "layout": {
    "title": "Owner Landing",
    "appColor": "white",
    "backgroundColor": "white"
  },
  "children": [
    {
      "component": "Container",
      "props": {
        "flexDirection": "column",
        "alignItems": "center",
        "justifyContent": "center",
        "flex": 1,
        "height": "100%",
        "maxWidth": 700
      },
      "children": [
        {
            "component": "Box",
            "props": {
                "marginBottom": 40
            },
            "children": [
                {
                    "component": "Image",
                    "props": {
                        "height": 100,
                        "width": 400,
                        "source": "https://raw.githubusercontent.com/genny-project/layouts/master/pcss/email_templates/images/SURETY_PPOPERTY2_CMYK.gif"
                    }
                }
            ]
        },
        {
          "component": "Box",
          "props": {
            "padding": 10
          },
          "children": [
            {
              "component": "Heading",
              "props": {
                "color": "#0592FD",
                "size": "xxl",
                "bold": false
              },
              "children": "Web Verification Code"
            }
          ]
        },
        {
          "component": "Box",
          "props": {
            "padding": 10
          },
          "children": [
            {
              "component": "Text",
              "props": {
                "size": "xs",
                "bold": false
              },
              "children": "Please enter the 5 alphanumeric code provided in the letter you received. If your code contains 0, it is the number zero."
            }
          ]
        },
        {
          "component": "Form",
          "props": {
            "questionGroupCode": "QUE_CODE_VERIFICATION_GRP",
            "renderSubheading": {
              "component": "Box",
              "props": {
                "marginY": 20
              },
              "children": [
                {
                  "component": "Heading",
                  "props": {
                    "color": "#0592FD",
                    "size": "lg",
                    "align": "left",
                    "text": "{{subheading}}"
                  }
                }
              ]
            },
            "renderFormInputWrapper": {
              "component": "Box",
              "props": {
                "marginBottom": 20,
                "flexDirection": "column"
              }
            },
            "renderFormInput": {
              "component": "FormInput",
              "useThemeFrom": "Input",
              "variant": "default",
              "props": {
                "placeholder": "{{label}}"
              }
            },
            "renderSubmitButtonWrapper": {
              "component": "Box",
              "props": {
                "marginTop": 10,
                "flexDirection": "column"
              }
            },
            "renderSubmitButton": {
              "component": "Button",
              "props": {
                "inverted": true,
                "showSpinnerOnClick": true
              }
            },
            "renderLoading": {
              "component": "Box",
              "props": {
                "flexDirection": "column"
              },
              "children": [
                {
                  "component": "ActivityIndicator",
                  "props": {
                    "size": "large"
                  }
                },
                {
                  "component": "Box",
                  "props": {
                    "marginTop": 10
                  },
                  "children": [
                    {
                      "component": "Text",
                      "props": {
                        "color": "#0592FD",
                        "text": "Loading form..."
                      }
                    }
                  ]
                }
              ]
            }
          }
        },
        {
          "component": "Box",
          "props": {
            "padding": 10
          },
          "children": [
            {
              "component": "Text",
              "props": {
                "size": "xs",
                "bold": false
              },
              "children": "For assistance, please contact Surety Property team on 1300 767 741 or send an email to bbaybridge@suretyproperty.com.au."
            }
          ]
        },
