{
  "children": [
    {
      "component": "Box",
      "props": {
        "width": "100%",
        "flex": 1,
        "position": "relative"
      },
      "children": [
        {
          "component": "Box",
          "props": {
            "flex": 1,
            "tabBarBackground": "#233a4e",
            "tabWrapperProps": {
              "paddingLeft": 20,
              "height": 50,
              "flexBasis": "auto"
            },
            "textColor": "#FFF",
            "tabBackground": "#41505d",
            "activeTabBackground": "#f6f6f6",
            "activeTabTextColor": "#000",
            "tabProps": {
              "paddingX": 20,
              "paddingY": 15,
              "minWidth": 180,
              "justifyContent": "center"
            },
            "tabTitleProps": {
              "size": "xs"
            },
            "activeTabTitleProps": {
              "bold": true
            },
            "inactiveTabProps": {
              "borderColor": "#233a4e",
              "borderRightWidth": 1,
              "borderStyle": "solid"
            },
            "tabs": [
            ],
            "childProps": {
              "overflow": "auto",
              "maxHeight": "calc(100vh - 65px - 50px)"
            },
            "overflowX": "auto",
            "backgroundColor": "#f6f6f6"
          },
          "children": [
            {
              "component":"Box",
              "props": {
                "width":"100%",
                "height":"100%",
                "flexDirection": "column",
                "alignItems": "center"
              },
              "children": [
                {
                  "component": "Sublayout",
                  "props": {
                    "layoutName": "home/dashboard/stats.students",
                    "showAllStudentsRow": true
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "query": [
    {
      "operator": "getLinks",
      "id": "GRP_NEW_ITEMS",
      "as": "items"
    },
    {
      "operator": "scope",
      "path": "items",
      "scope": {
        "operator": "getBE",
        "id": "{{link.targetCode}}"
      }
    },
    {
      "operator": "scope",
      "path": "items",
      "scope": {
        "operator": "find",
        "query": {
          "code": {
            "$exists": true
          }
        }
      }
    },
    {
      "operator": "scope",
      "path": "items",
      "scope": {
        "operator": "populateAttributes",
        "code": "{{code}}",
        "as": "attributes"
      }
    },
    {
      "operator": "scope",
      "path": "items",
      "scope": {
        "operator": "getBE",
        "id": "{{attributes.LNK_HOST_COMPANY.value}}",
        "as": "company"
      }
    },
    {
      "operator": "scope",
      "path": "items",
      "scope": {
        "operator": "scope",
        "path": "company",
        "scope": {
          "operator": "populateAttributes",
          "code": "{{code}}",
          "as": "attributes"
        }
      }
    }
  ]
}
