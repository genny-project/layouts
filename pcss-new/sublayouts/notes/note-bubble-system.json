{
  "children": [
    {
      "component": "Box",
      "props": {
        "flexDirection": "column",
        "marginBottom": 10
      },
      "children": [
        {
          "component": "Box",
          "props": {
            "justifyContent": "center",
            "marginBottom": 5
          },
          "children": [
            {
              "component": "Date",
              "props": {
                "date": "{{props.created}}",
                "format": "calendar",
                "color": "#fff",
                "align": "center",
                "size": "xxs"
              }
            }
          ]
        },
        {
          "component": "Box",
          "props": {
            "flexDirection": "column",
            "marginBottom": 15,
            "justifyContent": "center"
          },
          "children": [
            {
              "component": "Text",
              "props": {
                "text": "{{props.text}}",
                "color": "#fff",
                "align": "center",
                "size": "xs"
              }
            }
          ]
        }
      ]
    }
  ]
}
