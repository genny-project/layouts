{
  "children": [
    {
      "component": "Box",
      "props": {
        "width": "100%",
        "alignItems": "center",
        "justifyContent": "space-between",
        "backgroundColor": "white",
        "paddingY": 10
      },
      "children": [
        {
          "component": "Label",
          "props": {
            "text": "{{props.title}}",
            "color": "black",
            "size": "xs",
            "fontWeight": "bold"
          }
        },
        {
          "component": "Box",
          "props": {
            "marginLeft": "auto"
          },
          "children": [
            {
              "component": "Icon",
              "conditional": {
                "if": {
                  "props.isOpen": true
                },
                "then": {
                  "name": "remove"
                },
                "else": {
                  "name": "add"
                }
              },
              "props": {
                "color": "black",
                "size": "md"
              }
            }
          ]
        }
      ]
    }
  ]
}
