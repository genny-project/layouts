{
    "div":{
        "style":{
            "display":"flex",
            "flexDirection": "column"
        },
        "children":[
            {
                "Selector":{
                    "checkValues": ["BE.PRI_OWNER_PRICE_INC_GST"],
                    "showIfNull": true,
                    "children":[
                        {
                            "span":{
                                "style":{
                                    "fontSize":"1em",
                                    "paddingRight": "5px"
                                },
                                "children":"Negotiable"
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues": ["BE.PRI_OWNER_PRICE_INC_GST"],
                    "showIfNotNull": true,
                    "children":[
                        {
                            "span": {
                                "children":[
                                    {
                                        "Selector":{
                                            "checkValues": ["USER.PRI_IS_ADMIN"],
                                            "showValues": ["true"],
                                            "children":[
                                                {
                                                    "span":{
                                                        "style":{
                                                            "fontSize":"0.8em",
                                                            "paddingRight": "5px"
                                                        },
                                                        "children":"Owner: "
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "Selector":{
                                            "checkValues": ["USER.PRI_OWNER", "USER.PRI_IS_ADMIN"],
                                            "showValues": ["true"],
                                            "children":[
                                                {
                                                    "span":{
                                                        "style":{
                                                            "fontSize":"1em",
                                                            "paddingRight": "5px"
                                                        },
                                                        "children":"${BE.PRI_OWNER_PRICE_INC_GST} (incl GST)"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues": ["BE.PRI_DRIVER_PRICE_EXC_GST"],
                    "showIfNotNull": true,
                    "children":[
                        {
                            "span": {
                                "children":[
                                    {
                                        "Selector":{
                                            "checkValues": ["USER.PRI_IS_ADMIN"],
                                            "showValues": ["true"],
                                            "children":[
                                                {
                                                    "span":{
                                                        "style":{
                                                            "fontSize":"0.8em"
                                                        },
                                                        "children":"Driver: "
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "Selector":{
                                            "checkValues": ["USER.PRI_DRIVER", "USER.PRI_IS_ADMIN"],
                                            "showValues": ["true"],
                                            "children":[
                                                {
                                                    "span":{
                                                        "style":{
                                                            "fontSize":"1em"
                                                        },
                                                        "children":"${BE.PRI_DRIVER_PRICE_EXC_GST} (excl GST)"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues":["ROOT"],
                    "showValues":["GRP_NEW_ITEMS"],
                    "children":[
                        {
                            "Selector":{
                                "checkValues":["BE.PRI_OFFER_COUNT"],
                                "hideValues":["0"],
                                "children":[
                                    {
                                        "Selector":{
                                            "checkValues": ["USER.PRI_OWNER", "USER.PRI_IS_ADMIN"],
                                            "showValues": ["true"],
                                            "children":[
                                                {
                                                    "span":{
                                                        "className":"badge error",
                                                        "style":{
                                                            "fontSize":"12px"
                                                        },
                                                        "children":"BE.PRI_OFFER_COUNT"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    }
}
