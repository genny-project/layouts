{
  "Grid": {
    "rows": [
      {
        "style": {
          "flexGrow": 1,
          "margin": "10px"
        }
      }
    ],
    "cols": 3,
    "children": [
      {
        "div": {
          "style": {
            "display": "flex",
            "alignItems": "center",
            "height": "100%"
          },
          "position": [0, 0],
          "children": [
            {
              "IconSmall": {
                "name": "location_on",
                "style": {
                  "marginRight": "10px"
                }
              }
            },
            {
              "GennyAddressLabel": {
                "root": "BE.link.INTERNSHIP.code",
                "attributePrefix": "PRI_ADDRESS",
                "hideStreetAddress": true
              }
            }
          ]
        }
      },
      {
        "div": {
          "style": {
            "display": "flex",
            "alignItems": "center",
            "height": "100%"
          },
          "position": [0, 1],
          "children": [
            {
              "IconSmall": {
                "name": "date_range",
                "style": {
                  "marginRight": "10px"
                }
              }
            },
            {
              "DateLabel": {
                "date": "BE.link.INTERNSHIP.PRI_START_DATE",
                "format": "DD/MM/YYYY"
              }
            }
          ]
        }
      }
    ]
  }
}
