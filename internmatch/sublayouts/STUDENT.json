{
  "Grid": {
    "rows": 1,
    "cols": ["40px", 1],
    "style": {
      "background": "white",
      "fontSize": "10px"
    },
    "children": [
      {
        "div": {
          "position": [0, 0],
          "style": {
            "paddingRight": "10px",
            "paddingBottom": "5px"
          },
          "children": [
            {
              "ImageView": {
                "style": {
                  "margin": "0px",
                  "maxWidth": "40px",
                  "width": "40px"
                },
                "src": "BE.PRI_IMAGE_URL"
              }
            }
          ]
        }
      },
      {
        "div": {
          "position": [0, 1],
          "style": {
            "display": "flex",
            "alignItems": "center"
          },
          "children": [
            {
              "span": {
                "style": {
                  "marginRight": "5px"
                },
                "children": "BE.PRI_FIRSTNAME"
              }
            },
            {
              "span": {
                "children": "BE.PRI_LASTNAME"
              }
            }
          ]
        }
      },
      {
        "Selector": {
          "position": [0, 1],
          "checkValues": "BE.link.APPLICANT.PRI_STUDENT_ID",
          "showIfNotNull": true,
          "children": [
            {
              "p": {
                "position": [0, 0],
                "style": {
                  "margin": "0px"
                },
                "children": "ID: ${BE.link.APPLICANT.PRI_STUDENT_ID}"
              }
            }
          ]
        }
      },
      {
        "p": {
          "position": [0, 1],
          "style": {
            "margin": "0px"
          },
          "children": "BE.PRI_MOBILE"
        }
      }
    ]
  }
}
