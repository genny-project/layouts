{
  "layout": {
    "title": "Profile Education",
    "appColor": "dark"
  },
  "children": [
    {
      "component": "Box",
      "props": {
        "flexDirection": "row",
        "backgroundColor": "white",
        "flex": 1,
        "padding": 10,
        "marginBottom": 10,
        "alignItems": "center"
      },
      "children": [
        {
          "component": "Box",
          "props": {
            "marginRight": 20,
            "alignItems": "center"
          },
          "children": [
            {
              "component": "Image",
              "props": {
                "source": "{{query.attributes.PRI_IMAGE_URL.valueString}}",
                "shape": "circle",
                "width": 60,
                "height": 60
              }
            }
          ]
        },
        {
          "component": "Box",
          "props": {
            "flex": 1,
            "flexDirection": "column"
          },
          "children": [
            {
              "component": "Box",
              "props": {
                "marginBottom": 5
              },
              "children": [
                {
                  "component": "Text",
                  "props": {
                    "text": "{{query.attributes.PRI_CPY_NAME.valueString}}",
                    "color": "black",
                    "size": "sm"
                  }
                }
              ]
            },
            {
              "component": "Box",
              "props": {
                "marginBottom": 5
              },
              "children": [
                {
                  "component": "Text",
                  "props": {
                    "text": "{{query.attributes.PRI_POSITION.valueString}}",
                    "color": "black",
                    "size": "xxs"
                  }
                }
              ]
            },
            {
              "component": "Box",
              "props": {
                "marginBottom": 5
              },
              "children": [
                {
                  "component": "Text",
                  "props": {
                    "text": "{{query.attributes.PRI_ADDRESS_UNVERIFIED.valueString}}",
                    "color": "black",
                    "size": "xxs"
                  }
                }
              ]
            },
            {
              "component": "Box",
              "props": {
              },
              "children": [
                {
                  "component": "Text",
                  "props": {
                    "text": "{{query.attributes.PRI_START_DATE_UNVERIFIED.value}} - Current",
                    "color": "grey",
                    "size": "xxs"
                  },
                  "conditional": [
                    {
                      "if": {
                        "query.attributes.PRI_END_DATE_UNVERIFIED.value": {
                          "$exists": true
                        }
                      },
                      "then": {
                        "text": "{{query.attributes.PRI_START_DATE_UNVERIFIED.value}} - {{query.attributes.PRI_END_DATE_UNVERIFIED.value}}"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "component": "Link",
          "props": {
            "style": {
            },
            "to": "education/",
            "params": {
              "educationID": "{{props.educationID}}"
            }
          },
          "children": [
            {
              "component": "Box",
              "props": {
                "marginleft": 20,
                "alignItems": "center"
              },
              "children": [
                {
                  "component": "Box",
                  "props": {},
                  "children": [
                    {
                      "component": "Icon",
                      "props": {
                        "color": "black",
                        "size": "lg",
                        "name": "navigate-next"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "query": [
    {
      "operator": "getBE",
      "id": "{{props.educationID}}",
      "as": "be"
    },
    {
      "operator": "populateAttributes",
      "code": "{{props.educationID}}",
      "as": "attributes"
    }
  ]
}