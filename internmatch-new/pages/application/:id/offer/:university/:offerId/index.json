{
  "layout": {
    "title": "Application",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    },
    "sidebarRight": {
      "variant": "default"
    }
  },
  "children": [
    {
      "component": "Box",
      "props": {
        "marginX": "auto",
        "marginY": "100px",
        "width": "60%",
        "flex": 1,
        "flexDirection": "column"
      },
      "children": [
        {
          "component": "Box",
          "props": {
            "borderWidth": "1px",
            "borderColor": "grey",
            "borderStyle": "solid",
            "style": {
              "overflow": "scroll"
            }
            
          },
          "children": [
            {
              "component": "PdfViewer",
              "props": {
                "file": "{{query.application.linkValues.INT_AGMT.attributes.PRI_S3_AGREEMENT_LINK.value}}"
              }
            }
          ]
        },
        {
          "component": "Form",
          "props": {
            "questionGroupCode": "{{navigation.offerId}}",
            "style": {
              "width": "100%",
              "marginTop": "40px"
            },
            "renderSubmitButton": {
              "component": "Button",
              "props": {
                "showSpinnerOnClick": true,
                "color": "green",
                "width": "100%"
              }
            }
          }
        }
      ]
    }
  ],
  "query": [
    {
      "operator": "getBE",
      "id": "{{navigation.id}}",
      "as": "application"
    },
    {
      "operator": "scope",
      "path": "application",
      "scope": {
        "operator": "populateAttributes",
        "code": "{{code}}",
        "as": "attributes"
      }
    },
    {
      "operator": "scope",
      "path": "application",
      "scope": {
        "operator": "populateLinkValues",
        "withAttributes": true,
        "as": "linkValues"
      }
    }
  ]
}
