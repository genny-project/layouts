{
    "GennyDropdown":{
        "inline":true,
        "isSlide":false,
        "animateHeader":false,
        "animateFooter":true,
        "contentStyle":{
            "backgroundColor":"white",
            "padding":"0"
        },
        "header":[
            {
                "SublayoutLoader": {
                    "layoutCode": "card-header",
                    "aliases": {
                        "GROUP": "ROOT",
                        "BE": "BE.code"
                    }
                }
            }
        ],
        "footer":[
            {
                "SublayoutLoader": {
                    "layoutCode": "card-footer",
                    "aliases": {
                        "GROUP": "ROOT",
                        "BE": "BE.code"
                    }
                }
            }
        ],
        "children":[
            {
                "div":{
                    "style":{
                        "borderRadius":"10px",
                        "padding":"10px",
                        "background":"#ddd"
                    },
                    "children":[
                        {
                            "div":{
                                "style":{
                                    "paddingBottom":"5px"
                                },
                                "children":[
                                    {
                                        "h6":{
                                            "style":{
                                                "fontSize":"0.75em",
                                                "margin":"0"
                                            },
                                            "children":"PICKUP"
                                        }
                                    },
                                    {
                                        "SublayoutLoader":{
                                            "layoutCode":"address-pickup-display",
                                            "aliases":{
                                                "BE":"BE.code",
                                                "GROUP":"ROOT"
                                            }
                                        }
                                    },
                                    {
                                        "DateLabel":{
                                            "format":"DD/MM/YY HH:mm",
                                            "style":{
                                                "fontSize":"0.75em"
                                            },
                                            "children":"BE.PRI_PICKUP_DATETIME"
                                        }
                                    },
                                    {
                                        "h6":{
                                            "style":{
                                                "fontSize":"0.75em",
                                                "margin":"0"
                                            },
                                            "children":"DELIVERY"
                                        }
                                    },
                                    {
                                        "SublayoutLoader":{
                                            "layoutCode":"address-dropoff-display",
                                            "aliases":{
                                                "BE":"BE.code",
                                                "GROUP":"ROOT"
                                            }
                                        }
                                    },
                                    {
                                        "Selector":{
                                            "position":[
                                                1,
                                                1
                                            ],
                                            "checkValues": "BE.PRI_DROPOFF_DATETIME",
                                            "showIfNull":true,
                                            "children":[
                                                {
                                                    "span":{
                                                        "style":{
                                                            "fontSize":"0.75em"
                                                        },
                                                        "children":"TBC"
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "Selector":{
                                            "checkValues": "BE.PRI_DROPOFF_DATETIME",
                                            "showIfNotNull":true,
                                            "position":[
                                                1,
                                                1
                                            ],
                                            "children":[
                                                {
                                                    "DateLabel":{
                                                        "format":"DD/MM/YY HH:mm",
                                                        "style":{
                                                            "fontSize":"0.75em"
                                                        },
                                                        "children":"BE.PRI_DROPOFF_DATETIME"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "Grid":{
                                "cols":2,
                                "rows":2,
                                "children":[
                                    {
                                        "span":{
                                            "position":[
                                                0,
                                                0
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"L: "
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                0,
                                                0
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"BE.link.LOAD.PRI_LOAD_LENGTH_MM"
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                0,
                                                0
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":" mm"
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                0,
                                                1
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"W: "
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                0,
                                                1
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"BE.link.LOAD.PRI_LOAD_WIDTH_MM"
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                0,
                                                1
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":" mm"
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                1,
                                                0
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"H: "
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                1,
                                                0
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"BE.link.LOAD.PRI_LOAD_HEIGHT_MM"
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                1,
                                                0
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":" mm"
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                1,
                                                1
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"W: "
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                1,
                                                1
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"BE.link.LOAD.PRI_LOAD_WEIGHT_KG"
                                        }
                                    },
                                    {
                                        "span":{
                                            "position":[
                                                1,
                                                1
                                            ],
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":" kg"
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "Selector":{
                                "checkValues":["USER.PRI_OWNER", "USER.PRI_IS_ADMIN"],
                                "hideValues": "GRP_NEW_ITEMS",
                                "children":[
                                    {
                                        "GennyList":{
                                            "root":"BE.code",
                                            "showLinks":["ACCEPTED_OFFER", "DRIVER"],
                                            "itemsPerPage":1,
                                            "hideHeader":true,
                                            "hideCount":true,
                                            "hideNav":true,
                                            "showEmpty":false,
                                            "style":{
                                                "padding":"0"
                                            }
                                        }
                                    }
                                ]
                            }
                        },
                         {
                            "Selector":{
                                "checkValues":["USER.PRI_OWNER", "USER.PRI_IS_ADMIN"],
                                "showValues": "GRP_NEW_ITEMS",
                                "children":[
                                    {
                                        "Selector": {
                                            "checkValues": "ROOT",
                                            "hideValues": "GRP_NEW_ITEMS",
                                            "children": [
                                                 {
                                                    "GennyList":{
                                                        "root":"BE.code",
                                                        "showLinks":["ACCEPTED_OFFER", "DRIVER"],
                                                        "itemsPerPage":1,
                                                        "hideHeader":true,
                                                        "hideCount":true,
                                                        "hideNav":true,
                                                        "showEmpty":false,
                                                        "style":{
                                                            "padding":"0"
                                                        }
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                         {
                            "Selector":{
                                "checkValues":["USER.PRI_OWNER", "USER.PRI_IS_ADMIN", "USER.PRI_DRIVER"],
                                "showValues": true,
                                "children":[
                                    {
                                        "Selector": {
                                            "checkValues": "ROOT",
                                            "showValues": "GRP_NEW_ITEMS",
                                            "children": [
                                                 {
                                                    "GennyList":{
                                                        "root":"BE.code",
                                                        "showLinks":["OFFER"],
                                                        "itemsPerPage":1,
                                                        "hideHeader":true,
                                                        "hideCount":true,
                                                        "hideNav":true,
                                                        "showEmpty":false,
                                                        "style":{
                                                            "padding":"0"
                                                        }
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "Selector":{
                                "checkValues":[
                                    "USER.PRI_DRIVER",
                                    "USER.PRI_IS_ADMIN"
                                ],
                                "showValues": true,
                                "children":[
                                    {
                                        "GennyList":{
                                            "root":"BE.code",
                                            "showLinks": ["OWNER"],
                                            "itemsPerPage":10,
                                            "hideHeader":true,
                                            "hideCount":true,
                                            "showEmpty":false,
                                            "style":{
                                                "padding":"0"
                                            }
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "GennyButton":{
                                "buttonCode":"BTN_LOAD_SEE_MORE",
                                "value":{
                                    "itemCode":"BE.code",
                                    "hint":"ROOT"
                                },
                                "position":[
                                    0,
                                    1
                                ],
                                "style":{
                                    "height":"30px",
                                    "marginTop":"10px"
                                },
                                "buttonStyle":{
                                    "backgroundColor":"#000"
                                },
                                "children":[
                                    {
                                        "span":{
                                            "style":{
                                                "fontSize":"0.8em"
                                            },
                                            "children":"View Load Details"
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues": "ROOT",
                    "showValues": "GRP_APPROVED",
                    "children":[
                        {
                            "ProgressBar":{
                                "style":{
                                    "marginTop":"5px"
                                },
                                "type":2,
                                "progressTotal":100,
                                "progressCurrent":0
                            }
                        }
                    ]
                }
            },
            {
                "Selector":{
                    "checkValues": "ROOT",
                    "showValues": "GRP_DELIVERED",
                    "children":[
                        {
                            "ProgressBar":{
                                "style":{
                                    "marginTop":"5px"
                                },
                                "type":2,
                                "progressTotal":100,
                                "progressCurrent":100,
                                "progressPercent":"BE.PRI_PROGRESS"
                            }
                        }
                    ]
                }
            }
        ]
    }
}
