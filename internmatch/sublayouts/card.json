{
   "div": {
      "children": [
         {
            "Selector": {
               "checkValues": ["USER.PRI_IS_INTERN"],
               "showValues": ["true"],
               "children": [
                  {
                     "SublayoutLoader": {
                        "layoutCode": "card-intern",
                        "aliases": {
                           "BE": "BE.code",
                           "GROUP": "ROOT"
                        }
                     }
                  }
               ]
            }
         },
         {
            "Selector": {
               "checkValues": ["USER.PRI_IS_HOST_COMPANY"],
               "showValues": ["true"],
               "children": [
                  {  
                     "GennyDropdown":{  
                        "inline":true,
                        "isSlide":false,
                        "animateHeader":false,
                        "animateFooter":true,
                        "contentStyle":{  
                           "backgroundColor": "white",
                           "padding": "0"
                        },
                        "header":[ 
                           {
                              "SublayoutLoader": {
                                 "layoutCode": "card-content",
                                 "aliases": {
                                    "BE": "BE.code",
                                    "GROUP": "ROOT"
                                 }
                              }
                           }
                        ],
                        "footer":[
                           {  
                              "IconSmall":{ 
                                 "position": [1, 0],
                                 "name":"arrow_drop_down",
                                 "fontSize":"2em",
                                 "style":{  
                                    "padding":"0"
                                 }
                              }
                           }
                        ],
                        "children": [
                           {
                              "Selector": {
                                 "position":[2, 0],
                                 "checkValues": ["ROOT"],
                                 "showValues": ["GRP_AVAILABLE"],
                                 "children": [
                                     {  
                                       "GennyList":{  
                                          "root":"BE.code",
                                          "style":{  
                                             "marginTop":"10px"
                                          },
                                          "showLinks":[  
                                             "APPLICATION"
                                          ],
                                          "showEmpty":"false",
                                          "itemsPerPage":10
                                       }
                                    }
                                 ]
                              }
                           }
                        ]
                     }
                  }
               ]
            }
         },
         {
            "Selector": {
               "checkValues": ["USER.PRI_IS_EDU_PROVIDER"],
               "showValues": ["true"],
               "children": [
                  {
                     "Selector": {
                        "checkValues": ["ROOT"],
                        "showValues": ["GRP_INTERNS"],
                        "children": [
                           {  
                              "Grid":{  
                                 "cols": 1,
                                 "rows": 2,
                                 "children":[  
                                    {  
                                       "Grid":{  
                                          "position":[  
                                             0,
                                             0
                                          ],
                                          "rows":1,
                                          "cols":[  
                                             1,
                                             {
                                                "style": {
                                                   "flexGrow": 2,
                                                   "paddingTop": "5px"
                                                }
                                             }
                                          ],
                                          "children":[  
                                             {  
                                                "ImageView":{  
                                                   "position":[  
                                                      0,
                                                      0
                                                   ],
                                                   "src": "BE.PRI_IMAGE_URL",
                                                   "style":{  
                                                      "maxWidth": "40px",
                                                      "width": "40px"
                                                   }
                                                }
                                             },
                                             {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "style": {
                                                      "fontWeight": "bold"
                                                   },
                                                   "children":"BE.PRI_FIRSTNAME"
                                                }
                                             },
                                             {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "style": {
                                                      "fontWeight": "bold"
                                                   },
                                                   "children":" "
                                                }
                                             },
                                              {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "style": {
                                                      "fontWeight": "bold"
                                                   },
                                                   "children":"BE.PRI_LASTNAME"
                                                }
                                             },
                                             {
                                                "br": {
                                                    "position":[0, 1]
                                                }
                                             },
                                             {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "children":"BE.PRI_EMAIL"
                                                }
                                             },
                                              {
                                                "br": {
                                                    "position":[0, 1]
                                                }
                                             },
                                             {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "children":"BE.PRI_MOBILE"
                                                }
                                             }
                                          ]
                                       }
                                    }
                                 ]
                              }
                           }
                        ]
                     }
                  },
                  {
                     "Selector": {
                        "checkValues": ["ROOT"],
                        "hideValues": ["GRP_INTERNS"],
                        "children": [
                           {  
                              "Grid":{  
                                 "cols": 1,
                                 "rows": 2,
                                 "children":[
                                    {
                                       "Selector": {
                                          "position":[0,0],
                                          "checkValues": ["ROOT"],
                                          "showValues": ["GRP_OFFERED"],
                                          "children": [
                                             {
                                                 "GennyStatus": {
                                                     "root":"BE.code",
                                                     "style":{
                                                         "height":"25px",
                                                         "marginRight": "15%",
                                                         "float": "right"
                                                     },
                                                     "statusStyle":{
                                                         "height":"100%",
                                                         "maxHeight":"40px"
                                                     }
                                                 }
                                             }
                                          ]
                                       }
                                    },
                                    {  
                                       "Grid":{  
                                          "position":[  
                                             0,
                                             0
                                          ],
                                          "rows":1,
                                          "cols":[  
                                             1,
                                             {
                                                "style": {
                                                   "flexGrow": 2,
                                                   "paddingTop": "5px"
                                                }
                                             }
                                          ],
                                          "children":[  
                                             {  
                                                "ImageView":{  
                                                   "position":[  
                                                      0,
                                                      0
                                                   ],
                                                   "src": "BE.link.HOSTCOMPANY.PRI_IMAGE_URL",
                                                   "style":{  
                                                      "maxWidth": "40px",
                                                      "width": "40px"
                                                   }
                                                }
                                             },
                                             {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "children":"BE.link.INTERNSHIP.PRI_NAME"
                                                }
                                             },
                                              {
                                                "br": {
                                                    "position":[0, 1]
                                                }
                                             },
                                             {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "style": {
                                                      "fontWeight": "bold"
                                                   },
                                                   "children":"BE.link.INTERN.PRI_FIRSTNAME"
                                                }
                                             },
                                             {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "style": {
                                                      "fontWeight": "bold"
                                                   },
                                                   "children":" "
                                                }
                                             },
                                              {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "style": {
                                                      "fontWeight": "bold"
                                                   },
                                                   "children":"BE.link.INTERN.PRI_LASTNAME"
                                                }
                                             },
                                             {
                                                "br": {
                                                    "position":[0, 1]
                                                }
                                             },
                                             {  
                                                "span":{  
                                                   "position":[  
                                                      0,
                                                      1
                                                   ],
                                                   "children":"BE.link.INTERN.PRI_EMAIL"
                                                }
                                             }
                                          ]
                                       }
                                    }
                                 ]
                              }
                           }
                        ]
                     }
                  }
               ]
            }
         }                                 
      ]
   }
}
