{
  "layout": [
    {
      "AppHolder": {
        "cols": [0,5,0],
        "rows": 1,
        "children": [
          {
            "Sidebar": {
              "position": [0,0],
              "style": {
                "backgroundColor": "PROJECT.PRI_COLOR",
                "color": "white",
                "textAlign": "center"
              },
              "closeOnItemClick": false,
              "hasImage": true,
              "src": "PROJECT.PRI_LOGO",
              "children": [
                {
                  "GennyTreeView": {
                    "position": [1,0],
                    "style": {
                    "marginTop": "50px"
                  },
                    "root": "GRP_ROOT"
                  }
                }
              ]
            }
          },
          {
            "Grid": {
              "cols": [{"style": { "flexGrow" : 1, "width" : "100%"}}],
              "rows": ["45px", { "style" : {"flex": "1 1 auto", "overflow": "hidden" }}, "30px"],
              "position": [0,1],
              "className": "app-main",
              "children": [
                {
                  "GennyHeader": {
                    "position": [0,0],
                    "hideSubheader": true,
                    "style": {
                      "backgroundColor": "PROJECT.PRI_COLOR",
                      "color": "white"
                    },
                    "projectTitle" : "PROJECT.PRI_NAME",
                    "projectGreeting" : "PROJECT.PRI_GREETING",
                    "userName" : "USER.PRI_FIRSTNAME",
                    "userImage" : "USER.PRI_IMAGE_URL"
                  }
                },
                {
                  "Selector": {
                    "position": [
                      1,
                      0
                    ],
                    "checkValues": [
                      "USER.PRI_IS_BUYER",
                    ],
                    "showValues": [
                      true
                    ],
                    "children": [
                      {
                        "div": {
                          "style": {
                            "display":"flex",
                            "width": "100%",
                            "height": "40px",
                            "background": "#3f617f"
                          },
                          "children": [
                            {
                              "GennyButton": {
                                "buttonCode": "BTN_ADD_BEG",
                                "style": {
                                  "padding": "5px",
                                  "width": "initial",
                                  "height": "30px"
                                },
                                "buttonStyle": {
                                  "background": "#3f617f",
                                  "border": "1px solid white",
                                  "fontSize": "0.8em"
                                },
                                "value": {},
                                "children": "+ NEW INTERNSHIP"
                              }
                            },
                            {
                              "GennyButton": {
                                "buttonCode": "BTN_ADD_HOST_COMPANY",
                                "style": {
                                  "padding": "5px",
                                  "width": "initial",
                                  "height": "30px"
                                },
                                "buttonStyle": {
                                  "background": "#3f617f",
                                  "border": "1px solid white",
                                  "fontSize": "0.8em"
                                },
                                "value": {},
                                "children": "+ NEW HOST COMPANY"
                              }
                            }
                          ]
                        }
                      }
                    ]
                  }
                },
                {
                  "AppContent": {
                    "position": [1,0],
                    "children" : [
                      {
                        "div": {
                          "children": ""
                        }
                      }
                    ]
                  }
                },
                {
                  "Footer": {
                    "position": [2,0],
                    "style": {
                      "backgroundColor": "PROJECT.PRI_COLOR",
                      "color": "white"
                    },
                    "version" : "PROJECT.PRI_VERSION",
                    "poweredBy" : {
                      "caption" : "PROJECT.PRI_POWERED_BY"
                    }
                  }
                }
              ]
            }
          },
          {
            "Sidebar": {
              "position": [0,2],
              "style": {
                "backgroundColor": "red",
                "color": "white",
                "textAlign": "center"
              },
              "closeOnItemClick": false,
              "hasImage": false,
              "slideFromRight": true,
              "children": [
                {
                  "h2": {
                    "position": [1,0],
                    "style": {
                      "marginTop": "50px"
                    },
                    "children": "NOTES"
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
