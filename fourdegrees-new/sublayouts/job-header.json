{
  "sublayout": {
    "title": "Job-Header",
    "appColor": "dark"
  },
  "children": [
    {
      "component": "Box",
      "props": {
        "flexDirection": "row",
        "alignItems": "center",
        "justifyContent": "center",
        "flex": 1,
        "height": "100%"
      },
      "children": [
        {
          "component": "Box",
          "props": {
          },
          "children": [
          {
              "component": "Image",
              "props": {
                "source": "{{BE.link.LOAD.PRI_IMAGE_URL}}"
              }
            }
          ]
        },
        {
          "component": "Box",
          "props": {
            "flexDirection": "column",
            "alignItems": "center",
            "justifyContent": "center",
            "flex": 1,
            "height": "100%"
          },
          "children": [
            {
              "component": "Label",
              "props": {
                "text": "{{BE.link.LOAD.PRI_NAME}}"
              }
            },
            {
              "component": "Label",
              "props": {
                "text": "${{BE.link.LOAD.PRI_PRICE}} (gst)"
              }
            },
            {
              "component": "Label",
              "props": {
                "text": "Posted: {{BE.created}} at {{BE.created}}"
              }
            }
          ]
        }
      ]
    }
  ]
}
