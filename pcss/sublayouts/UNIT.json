{
  "Grid": {
    "rows": 2,
    "cols": 1,
    "style": {
      "padding": "0",
      "background": "white",
      "fontSize": "10px"
    },
    "children": [
      {
        "Selector": {
          "position": [0, 0],
          "checkValues": ["BE.linkValue"],
          "showValues": ["UNIT"],
          "children": [
            {  
              "GennyAddressLabel":{  
                 "position":[  
                    0,
                    1
                 ],
                 "style":{  

                    "fontSize":"0.85em"
                 },
                 "attributePrefix":"PRI_ADDRESS",
                 "root":"BE.code"
              }
           },
            {
              "Selector": {
                "checkValues": ["USER.PRI_IS_BUYER"],
                "showValues": [true,"true"],
                "position": [0, 0],
                "children": [
                  {
                    "SublayoutLoader": {
                      "layoutCode": "card",
                      "aliases": {
                        "BE": "BE.code",
                        "GROUP": "ROOT"
                      }
                    }
                  }
                ]
              }
            },
            {
              "Selector": {
                "checkValues": ["USER.PRI_IS_JOHN_HOLLAND"],
                "showValues": [true,"true"],
                "position": [0, 0],
                "children": [
                  {
                    "SublayoutLoader": {
                      "layoutCode": "card",
                      "aliases": {
                        "BE": "BE.code",
                        "GROUP": "ROOT"
                      }
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  }
}
