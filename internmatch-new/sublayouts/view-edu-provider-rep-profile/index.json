{
  "layout": {
    "title": "Edu-Provider-Rep Profile",
    "appColor": "white",
    "backgroundColor": "silver"
  },
  "children": [
  {
    "component": "Container",
    "props": {
      "flexDirection" : "column",
      "justifyContent": "center",
      "marginTop":"20px",
      "minWIdth": "900px",
      "width":"100%"
    },
    "children": [
      {
        "component": "Box",
        "props":{
          "flexDirection":"column",
          "backgroundColor": "rgb(255,255,255)"
        },
        "children":[
          {
            "component": "Box",
            "props": {
              "flexDirection": "row",
              "width":"100%",
              "backgroundColor": "white"
            },
            "children": [
              {
                "component": "Box",
                "props": {
                  "paddingTop": "20px",
                  "paddingBottom": "20px",
                  "paddingLeft": "20px",
                  "paddingRight": "10px"
                },
                "children":          
                {
                  "component": "Image",
                  "props": {
                    "source": "{{props.imageUrl}}",
                    "shape": "square",
                    "height":150,
                    "width":150
                  }
                }
              },
              {
                "component": "Box",
                "props": {
                  "flexDirection": "column",
                  "justifyContent": "center",
                  "marginLeft": "20px"
                },
                "children":
                [
                  {
                    "component":"Box",
                    "props": {
                      "flexDirection": "column"
                    },
                    "children": [
                      {
                        "component": "Text",
                        "props": {
                          "color":"#000000",
                          "size":"xl",
                          "bold": true
                        },
                        "children": "{{props.fullName}}"
                      }
                    ]
                  },
                  {
                    "component":"Box",
                    "props": {
                      "flexDirection": "column",
                      "marginTop": "20px"
                    },
                    "children": [
                      {
                        "component": "Text",
                        "props": {
                          "color":"rgb(5, 88, 188)",
                          "size":"md",
                          "bold": false
                        },
                        "children": "{{props.universityName}}"
                      }
                    ]
                  }
                ]        
              }
            ]
          },
          {
            "component": "Box",
            "props": {
              "backgroundColor": "silver",
              "height": "1px"
            }
          },
          {
            "component":"Box",
            "props":{
              "paddingTop": "20px",
              "paddingLeft":"20px",
              "marginBottom": "20px"
            },
            "children":[
              {
                "component": "Box",
                "props":{
                  "borderWidth": "1px",
                  "borderColor": "rgb(85, 148, 188)"
                },
                "children":
                {
                  "component": "EventButton",
                  "props": {
                    "height": "50px",
                    "width": "150px",
                    "borderRadius": 30,
                    "showSpinnerOnClick": true,
                    "color": "rgb(85, 148, 188)",
                    "textColor": "#fff",
                    "text": "Send Email",
                    "buttonCode": "",
                    "value": {
                      "itemCode": "{{}}"
                    }
                  }
                }
              },
              {
                "component": "Box",
                "props":{
                  "marginLeft": "20px",
                  "borderWidth": "1px",
                  "borderColor": "rgb(85, 148, 188)"                },
                "children":
                {
                  "component": "EventButton",
                  "props": {
                    "height": "50px",
                    "width": "150px",
                    "backgroundColor": "red",
                    "color": "rgb(255, 255, 255)",
                    "textColor": "rgb(85, 148, 188)",
                    "textSize": "xl",
                    "text": "Resume",
                    "buttonCode": "",
                    "value": {
                      "itemCode": "{{}}"
                    }
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "component": "Box",
        "props": {
          "flexDirection":"column",
          "backgroundColor": "rgb(255,255,255)",
          "marginTop":"20px"
      },
        "children":
      [
        {
          "component": "Box",
          "props":{
            "paddingTop": "10px",
            "paddingLeft": "20px",
            "paddingBottom": "10px"
          },
          "children":
          {
            "component": "Text",
            "props":{
              "size": "xl"
            },
            "children": "Contact"
          }
        },
        {
          "component": "Box",
          "props": {
            "backgroundColor": "silver",
            "height": "1px"
          }
        },
        {
          "component":"Box",
          "props":{
            "flexDirection":"column",
            "marginX": "20px",
            "marginY": "20px",
            "justifyContent": "space-between",
            "height":"45px"
          },
          "children":[
            {
              "component": "Text",
              "props": {
                "bold": true
              },
              "children": "Email:"
            },
            {
          "component": "Text",
          "props": {
            "text": "{{props.email}}"
          }
            }
          ]
        },
        {
          "component":"Box",
          "props":{
            "flexDirection":"column",
            "marginX": "20px",
            "marginY": "20px",
            "justifyContent": "space-between",
            "height":"45px"
          },
          "children":[
            {
              "component": "Text",
              "props": {
                "bold": true
              },
              "children": "Mobile:"
            },
            {
          "component": "Text",
          "props": {
            "text": "{{props.attributes.PRI_MOBILE.value}}"
          }
        }
          ]
        }
      ]
      }
    ]
  }
 ]
}

