{
    "children": [
      {
        "component": "Box",
        "children": [
          {
            "component": "Table",
            "layoutName": "edit-properties",
            "props": {
                "columns": [
                    {
                      "Header": "Action",
                      "editable": true,
                      "accessor": "actions",
                      "renderButton": [
                        {
                          "component": "EventButton",
                          "props": {
                            "buttonCode": "BTN_VIEW_BEG",
                            "color": "#5fa5ec",
                            "textColor": "#fff",
                            "text": "Property Details",
                            "padding": 5,
                            "size": "xs",
                            "value": {
                              "itemCode":"{{code}}",
                              "hint": "BTN_VIEW_BEG"
                            }
                          }
                        }
                      ]
                    },
                    {
                      "Header": "Address",
                      "accessor": "address",
                      "filterType": "string"
                    },  
                    {
                      "Header": "Precinct",
                      "accessor": "precinct",
                      "filterType": "string"
                    },
                    {
                      "Header": "Property Type",
                      "accessor": "propertytype",
                      "filterType": "string"
                    }
                  ],
              "tableBackgroundColor": "#e05b5b",
              "data": "_query.tableData",
              "height": "500px",
              "width": "500px",
              "itemsPerPage": 15
            }
          }
        ]
      }
    ],
    "query": [
        {
          "operator": "getBE",
          "id": "GRP_NEW_ITEMS",
          "as": "items"
        },
        {
          "operator": "scope",
          "path": "items.links",
          "as": "items",
          "scope": {
            "operator": "getBE",
            "basePath": "baseEntities.data",
            "id": "{{link.targetCode}}",
            "as": "be"
          }
        },
        {
          "operator": "scope",
          "path": "items",
          "scope": {
            "operator": "scope",
            "path": "be",
            "scope": {
              "operator": "populateBEGAttributes",
              "as": "attributes"
            }
          }
        },
        {
          "operator": "scope",
          "path": "items",
          "scope": {
            "operator": "getBE",
            "as" : "company",
            "basePath": "baseEntities.data",
            "id": "{{be.attributes.LNK_PROPERTY_PRECINCT_SELECT.value}}"
          }
        },
        {
          "operator": "scope",
          "path": "items",
          "scope": {
            "operator": "getBE",
            "as" : "items",
            "id": "{{be.attributes.LNK_PROPERTY_TYPE_SELECT.value}}"
          }
        },
        {
          "operator": "scope",
          "path": "items",
          "scope": {
            "operator": "scope",
            "path": "items",
            "scope": {
              "operator": "populateAttributes",
              "as" : "attributes",
              "code": "{{code}}"
            }
          }
        }
      ]
  }
  