{
  "layout": {
    "title": "{{query.intern.attributes.PRI_NAME.value}}",
    "appColor": "white",
    "backgroundColor": "whitesmoke",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    }
  },
  "children": [
    {
      "component": "Box",
      "props": {
        "width": "100%",
        "flex": 1,
        "position": "relative",
        "backgroundColor": "#f6f6f6"
      },
      "children": [
        {
          "component": "Tabs",
          "props": {
            "flex": 1,
            "tabBarBackground": "#233a4e",
            "tabWrapperProps": {
              "paddingLeft": 20,
              "height": 50,
              "flexBasis": "auto"
            },
            "textColor": "#FFF",
            "tabBackground": "#41505d",
            "activeTabBackground": "#f6f6f6",
            "activeTabTextColor": "#000",
            "tabProps": {
              "paddingX": 20,
              "paddingY": 15,
              "minWidth": 180,
              "justifyContent": "center"
            },
            "tabTitleProps": {
              "size": "xs"
            },
            "activeTabTitleProps": {
              "bold": true
            },
            "inactiveTabProps": {
              "borderColor": "#233a4e",
              "borderRightWidth": 1,
              "borderStyle": "solid"
            },
            "tabs": [
              {
                "title": "Process View",
                "key": 0
              },
              { "title": "Internship Details", "key": 1 }
            ],
            "childProps": {
              "overflow": "auto",
              "maxHeight": "calc(100vh - 65px - 50px)"
            },
            "overflowX": "auto",
            "backgroundColor": "#f6f6f6"
          },
          "children": [
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "home/agent/bucket"
              }
            },
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "profile/intern"
              }
            }
          ]
        }
      ]
    }
  ],
  "query": [{
      "operator": "getFromAlias",
      "alias": "CURRENT_NOTE",
      "as": "intern"
    },
    {
      "operator": "scope",
      "path": "intern",
      "scope": {
        "operator": "populateAttributes",
        "code": "{{code}}",
        "as": "attributes"
      }
    }
  ]
}
