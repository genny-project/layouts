{
  "rootCode": "GRP_ROOT",
  "width": 300,
  "flex": 1,
  "inline": true,
  "renderBody": {
    "component": "Box",
    "props": {
      "backgroundColor": "#ffffff",
      "position": "fixed",
      "width": 300,
      "height": "100vh",
      "left": 0,
      "top": 0,
      "flexDirection": "column"
    },
    "children": [
      {
        "component": "Box",
        "props": {
          "padding": 20,
          "paddingBottom": 0,
          "height": 115
        },
        "children": [
          {
            "component": "Image",
            "props": {
              "source": "https://puu.sh/BB7km.png",
              "height": 70
            }
          }
        ]
      },
      {
        "component": "Box",
        "props": {
          "flexDirection": "column"
        },
        "repeat": "_items",
        "children": {
          "component": "Touchable",
          "props": {
            "padding": 20,
            "paddingLeft": 50,
            "onPress": "_repeater.onPress",
            "withFeedback": true,
            "alignItems": "center"
          },
          "children": {
            "component": "Box",
            "children": [
              {
                "component": "Box",
                "props": {
                  "marginRight": 15
                },
                "children": [
                  {
                    "component": "Icon",
                    "props": {
                      "name": "{{repeater.icon}}",
                      "color": "#959595"
                    }
                  }
                ]
              },
              {
                "component": "Text",
                "props": {
                  "color": "#959595"
                },
                "children": "{{repeater.name}}"
              }
            ]
          }
        }
      }
    ]
  }
}
