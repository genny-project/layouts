          "props": {
            "layoutName": "tables/table-default",
            "columns": [
              {
                "Header": "Address",
                "accessor": "address",
                "filterType": "string"
              },
              {
                "Header": "Property Location",
                "accessor": "precinct",
                "filterType": "string"
              },
              {
                "Header": "Property Type",
                "accessor": "propertytype",
                "filterType": "string"
              },
              {
                "Header": "Action",
                "editable": true,
                "accessor": "actions",
                "renderButton": [
                  {
                    "component": "EventButton",
