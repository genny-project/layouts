{
  "layout": [
    {
      "AppHolder": {
        "sidebar": {
          "style": {
            "backgroundColor": "PROJECT.PRI_COLOR",
            "color": "white",
            "textAlign": "center"
          },
          "hasImage": true,
          "caption": "USER.PRI_FIRSTNAME",
          "src": "PROJECT.PRI_LOGO"
        },
        "header": {
          "style": {
            "backgroundColor": "PROJECT.PRI_COLOR",
            "color": "white"
          },
          "projectTitle" : "PROJECT.PRI_NAME",
          "userName" : "USER.PRI_FIRSTNAME",
          "userImage" : "USER.PRI_IMAGE_URL"
        },
        "footer" : {
          "style": {
            "backgroundColor": "PROJECT.PRI_COLOR",
            "color": "white"
          },
          "version" : "0.0.0.1",
          "poweredBy" : {
            "caption" : "PROJECT.PRI_POWERED_BY"
          }
        },
        "children": [
          {
            "GennyTreeView": {
              "style": {
                "marginTop": "50px"
              },
              "root": "GRP_ROOT"
            }
          },
          {
            "div": {
              "style": {
                "backgroundColor": "PROJECT.PRI_COLOR",
                "color": "#FFF",
                "padding": "20px",
                "fontFamily": "Helvetica",
                "fontWeight": "400"
              },
              "className" : "sticky",
              "children": [
                {
                  "h3": {
                    "children": "Project Title",
                    "style": {
                      "marginBottom": "10px",
                      "marginTop": "10px"
                    }
                  }
                }
              ]
            }
          },
          {
            "GennyForm": {
              "itemsPerPage" : 10,
              "showProgress" : true,
              "style": {
                "color": "PROJECT.PRIMARY_COLOR"
              }
            }
          },
          {
            "GennyBucketView": {
              "root": "GRP_DRIVER_VIEW"
            }
          },
          {
            "GennyList": {
              "root": "GRP_QUOTES"
            }
          },
          {
            "GennyTable": {
              "root": "GRP_USERS"
            }
          }
        ]
      }
    }
  ]
}
