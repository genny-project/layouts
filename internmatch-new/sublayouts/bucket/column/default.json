{
  "layout": {
    "title": "Bucket Column Default",
    "appColor": "white",
    "backgroundColor": "white"
  },
  "children": [
    {
      "component": "List",
      "props": {
        "headerText": "{{repeater.name}}",
        "contentBackground": "#eaeaea",
        "headerProps": {
          "paddingX": 20,
          "paddingY": 15,
          "justifyContent": "space-between",
          "alignItems": "center",
          "backgroundColor": "#eaeaea",
          "position": "sticky",
          "top": 0
        },
        "headerTextProps": {
          "bold": true
        },
        "showCountInHeader": true,
        "contentBodyProps": {
          "paddingX": 10,
          "paddingY": 20
        },
        "emptyProps": {
          "padding": 10
        },
        "headerCountTextProps": {
          "bold": true,
          "size": "xs"
        },
        "emptyTextProps": {
          "size": "xs"
        },
        "scrollViewProps": {
          "horizontal": true
        }
      },
      "repeat": "_repeater.items",
      "sort": {
        "by": "repeater.created",
        "isDate": true
      },
      "children":
    }
  ]
}