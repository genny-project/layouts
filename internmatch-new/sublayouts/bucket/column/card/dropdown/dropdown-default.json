{
  "children": [
    {
      "component": "Dropdown",
      "props": {
        "facingRight": true,
        "menuButtonStyle": {
          "backgroundColor": "transparent",
          "border": 0,
          "padding": 5,
          "fontSize": 16,
          "cursor": "pointer",
          "color": "white"
        },
        "menuListStyle": {
          "background": "#233a4e",
          "borderWidth": 2,
          "borderColor": "#192835",
          "borderStyle": "solid"
        },
        "menuItemStyle": {
          "textAlign": "right",
          "padding": 15,
          "color": "white"
        }
      },
      "conditional": {
        "if": {
          "repeater.attributes.PRI_IS_DEACTIVATED.value": {
            "$eq": true
          }
        },
        "then": {
          "items": [
            {
              "text": "Re-Activate",
              "buttonCode": "BTN_ACTIVATE_APPLICATION",
              "value": {
                "itemCode": "{{repeater.code}}",
                "hint": "{{parentRepeater.code}}"
              }
            }
          ]
        },
        "else": {
          "if": {
            "repeater.attributes.PRI_GRP_IN_PROGRESS_DATE.value": {
              "$exists": true,
              "$ne": "undefined"
            }
          },
          "then": {
            "items": [
              {
                "text": "View Application",
                "href": "/application/{{repeater.code}}"
              },
              {
                "text": "View Internship Offer",
                "buttonCode": "BTN_SHOW_COMPLIANCE_DOCUMENT",
                "value": {
                  "itemCode": "{{repeater.code}}",
                  "hint": "{{parentRepeater.code}}"
                }
              },
              {
                "text": "Finish Internship",
                "buttonCode": "BTN_FINISH_INTERNSHIP",
                "value": {
                  "itemCode": "{{repeater.code}}",
                  "hint": "{{parentRepeater.code}}"
                }
              },
              {
                "text": "On Hold",
                "buttonCode": "BTN_DEACTIVATE_APPLICATION",
                "value": {
                  "itemCode": "{{repeater.code}}",
                  "hint": "{{parentRepeater.code}}"
                }
              }
            ]
          },
          "else": {
            "if": {
              "repeater.attributes.PRI_GRP_PLACED_DATE.value": {
                "$exists": true,
                "$ne": "undefined"
              }
            },
            "then": {
              "items": [
                {
                  "text": "View Application",
                  "href": "/applications/{{repeater.code}}"
                },
                {
                  "text": "View Internship Offer",
                  "buttonCode": "BTN_SHOW_COMPLIANCE_DOCUMENT",
                  "value": {
                    "itemCode": "{{repeater.code}}",
                    "hint": "{{parentRepeater.code}}"
                  }
                },
                {
                  "text": "Start Internship",
                  "buttonCode": "BTN_START_INTERNSHIP",
                  "value": {
                    "itemCode": "{{repeater.code}}",
                    "hint": "{{parentRepeater.code}}"
                  }
                },
                {
                  "text": "On Hold",
                  "buttonCode": "BTN_DEACTIVATE_APPLICATION",
                  "value": {
                    "itemCode": "{{repeater.code}}",
                    "hint": "{{parentRepeater.code}}"
                  }
                },
                {
                  "text": "Reject",
                  "buttonCode": "BTN_REJECT_APPLICATION",
                  "value": {
                    "itemCode": "{{repeater.code}}",
                    "hint": "{{parentRepeater.code}}"
                  }
                }
              ]
            },
            "else": {
              "if": {
                "repeater.attributes.PRI_GRP_OFFERED_DATE.value": {
                  "$exists": true,
                  "$ne": "undefined"
                }
              },
              "then": {
                "items": [
                  {
                    "text": "View Application",
                    "href": "/application/{{repeater.code}}"
                  },
                  {
                    "text": "View Internship Offer",
                    "buttonCode": "BTN_SHOW_COMPLIANCE_DOCUMENT",
                    "value": {
                      "itemCode": "{{repeater.code}}",
                      "hint": "{{parentRepeater.code}}"
                    }
                  },
                  {
                    "text": "On Hold",
                    "buttonCode": "BTN_DEACTIVATE_APPLICATION",
                    "value": {
                      "itemCode": "{{repeater.code}}",
                      "hint": "{{parentRepeater.code}}"
                    }
                  }
                ]
              },
              "else": {
                "if": {
                  "repeater.attributes.PRI_GRP_INTERVIEWS_DATE.value": {
                    "$exists": true,
                    "$ne": "undefined"
                  }
                },
                "then": {
                  "items": [
                    {
                      "text": "View Application",
                      "href": "/application/{{repeater.code}}"
                    },
                    {
                      "text": "Make Offer",
                      "buttonCode": "BTN_OFFER_APPLICATION",
                      "value": {
                        "itemCode": "{{repeater.code}}",
                        "hint": "{{parentRepeater.code}}"
                      }
                    },
                    {
                      "text": "On Hold",
                      "buttonCode": "BTN_DEACTIVATE_APPLICATION",
                      "value": {
                        "itemCode": "{{repeater.code}}",
                        "hint": "{{parentRepeater.code}}"
                      }
                    }
                  ]
                },
                "else": {
                  "if": {
                    "repeater.attributes.PRI_GRP_SHORTLISTED_DATE.value": {
                      "$exists": true,
                      "$ne": "undefined"
                    }
                  },
                  "then": {
                    "items": [
                      {
                        "text": "View Application",
                        "href": "/application/{{repeater.code}}"
                      },
                      {
                        "text": "Interview",
                        "buttonCode": "BTN_INTERVIEW_APPLICATION",
                        "value": {
                          "itemCode": "{{repeater.code}}",
                          "hint": "{{parentRepeater.code}}"
                        }
                      },
                      {
                        "text": "Reject",
                        "buttonCode": "BTN_REJECT_APPLICATION",
                        "value": {
                          "itemCode": "{{repeater.code}}",
                          "hint": "{{parentRepeater.code}}"
                        }
                      },
                      {
                        "text": "On Hold",
                        "buttonCode": "BTN_DEACTIVATE_APPLICATION",
                        "value": {
                          "itemCode": "{{repeater.code}}",
                          "hint": "{{parentRepeater.code}}"
                        }
                      }
                    ]
                  },
                  "else": {
                    "if": {
                      "repeater.attributes.PRI_GRP_APPLIED_DATE.value": {
                        "$exists": true,
                        "$ne": "undefined"
                      }
                    },
                    "then": {
                      "items": [
                        {
                          "text": "View Application",
                          "href": "/application/{{repeater.code}}"
                        },
                        {
                          "text": "Shortlist",
                          "buttonCode": "BTN_SHORTLIST_APPLICATION",
                          "value": {
                            "itemCode": "{{repeater.code}}",
                            "hint": "{{parentRepeater.code}}"
                          }
                        },
                        {
                          "text": "On Hold",
                          "buttonCode": "BTN_DEACTIVATE_APPLICATION",
                          "value": {
                            "itemCode": "{{repeater.code}}",
                            "hint": "{{parentRepeater.code}}"
                          }
                        }
                      ]
                    },
                    "else": {
                      "items": [
                        {
                          "text": "View Student",
                          "href": "/student/{{repeater.code}}"
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      },
      "children": [
        {
          "component": "Box",
          "children": [
            {
              "component": "Icon",
              "props": {
                "name": "expand_more",
                "color": "#233a4e",
                "size": "smd"
              }
            }
          ]
        }
      ]
    }
  ]
}