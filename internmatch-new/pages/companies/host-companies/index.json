{
  "layout": {
    "title": "All Host Companies",
    "appColor": "white",
    "backgroundColor": "whitesmoke",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    },
    "sidebarRight": {
      "variant": "default",
      "onlyShowIf": {
        "$or": [
          {
            "user.attributes.PRI_IS_AGENT.value": {
              "$eq": true
            }
          },
          {
            "user.attributes.PRI_IS_EDU_PROVIDER_STAFF.value": {
              "$eq": true
            }
          },
          {
            "user.attributes.PRI_IS_BUYER.value": {
              "$eq": true
            }
          }
        ]
      }
    }
  },
  "children": [
    {
      "component": "Sublayout",
      "props": {
        "layoutName": "tabs/default",
        "title": "All Host Companies",
        "tabs": [
          { "title": "Table View", "key": 0 },
          { "title": "Details View", "key": 1 }
        ],
        "content": [
          {
            "component": "Sublayout",
            "props": {
              "layoutName": "tables/companies/host-companies"
            }
          },
          {
            "component": "Sublayout",
            "props": {
              "layoutName": "view-details/company"
            }
          }
        ]
      }
    }
  ]
}