                    },
                    "children": [
                      {
                        "component": "Box",
                        "props": {
                        },
                        "children": [
                          {
                            "component": "Box",
                            "props": {
                            },
                            "children": [
                              {
                                "component": "Icon",
                                "props": {
                                  "name": "delete",
                                  "color": "red"
                                }
                              }
                            ]
                          },
                          {
                            "component": "Text",
                            "props": {
                              "text": "",
                              "size": "sm",
                              "color": "white"
                            }
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ],
            "tableBackgroundColor": "#e05b5b",
            "data": "_query.tableData",
            "height": "500px",
            "rootCode": "GRP_NEW_ITEMS",
            "rootGroup": "GRP_NEW_ITEMS"
          }
        }
      ]
    }
  ],
  "query": [
    {
      "operator": "getBE",
      "id": "GRP_NEW_ITEMS",
      "as": "identifiedProperties"
    },
    {
      "operator": "scope",
      "path": "identifiedProperties.links",
      "as": "identifiedProperties",
      "scope": {
        "operator": "getBE",
        "basePath": "baseEntities.data",
        "id": "{{link.targetCode}}",
        "as": "properties"
      }
    },
    {
      "operator": "scope",
      "path": "identifiedProperties",
      "scope": {
        "operator": "scope",
        "path": "properties",
        "scope": {
          "operator": "populateBEGAttributes",
          "as": "attributes"
        }
      }
    },
    {
      "operator": "scope",
      "path": "identifiedProperties",
      "scope": {
        "operator": "scope",
        "path": "properties",
        "scope": {
          "operator": "scope",
          "path": "attributes",
          "scope": {
            "operator": "getBE",
            "basePath": "baseEntities.data",
            "id": "{{LNK_PROPERTY_PRECINCT_SELECT.value}}",
            "as": "PrecinctTypeBE"
          }
        }
      }
    },
    {
      "operator": "scope",
      "path": "identifiedProperties",
      "scope": {
        "operator": "scope",
        "path": "properties",
        "scope": {
          "operator": "scope",
          "path": "attributes",
          "scope": {
            "operator": "getBE",
            "basePath": "baseEntities.data",
            "id": "{{LNK_PROPERTY_TYPE_SELECT.value}}",
            "as": "PropertyTypeNameBE"
          }
        }
      }
    },
    {
      "operator": "scope",
      "path": "identifiedProperties",
      "as": "tableData",
      "scope": {
        "operator": "map",
        "fields": {
          "address": "properties.attributes.PRI_ADDRESS_FULL.value",
          "precinct": "properties.attributes.PrecinctTypeBE.name",
          "propertytype": "properties.attributes.PropertyTypeNameBE.name",
          "code": "properties.code"
        }
      }
    },
    {
      "operator": "clean",
      "fields": ["tableData"]
    }
  ]
}
