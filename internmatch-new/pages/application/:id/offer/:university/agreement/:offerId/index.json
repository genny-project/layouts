{
  "layout": {
    "title": "Agreement Document",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    },
    "sidebarRight": {
      "variant": "default",
      "onlyShowIf": {
        "$or": [
          {
            "user.attributes.PRI_IS_AGENT.value": {
              "$eq": true
            }
          },
          {
            "user.attributes.PRI_IS_EDU_PROVIDER_STAFF.value": {
              "$eq": true
            }
          },
          {
            "user.attributes.PRI_IS_BUYER.value": {
              "$eq": true
            }
          }
        ]
      }
    }
  },
  "children": [
    {
      "component": "Box",
      "props": {
        "marginX": "auto",
        "marginY": "100px",
        "maxWidth": "700px",
        "minWidth": "500px",
        "flex": 1,
        "flexDirection": "column"
      },
      "children": [
        {
          "component": "Box",
          "props": {
            "width": "100%"
          },
          "children": [
            {
              "component": "DisplayHTML",
              "props": {
                "html": "{{query.application.linkValues.INT_AGMT.attributes.PRI_S3_AGREEMENT_HTML.value}}"
              },
              "onlyShowIf": {
                "query.application.linkValues.INT_AGMT.attributes.PRI_S3_AGREEMENT_HTML.value": {
                  "$exists": true
                }
              }
            },
            {
              "component": "PdfViewer",
              "props": {
                "file": "{{query.application.linkValues.INT_AGMT.attributes.PRI_S3_AGREEMENT_LINK.value}}"
              },
              "onlyShowIf": {
                "query.application.linkValues.INT_AGMT.attributes.PRI_S3_AGREEMENT_HTML.value": {
                  "$exists": false
                }
              }
            }
          ]
        }
      ]
    },
    {
      "component": "Form",
      "onlyShowIf": {
        "$or": [
          {
            "$and": [
              {
                "user.attributes.PRI_IS_BUYER.value": {
                  "$eq": true
                }
              },
              {
                "query.document.attributes.PRI_HOST_COMPANY_ACCEPTED.value": {
                  "$ne": true
                }
              }
            ]
          },
          {
            "$and": [
              {
                "user.attributes.PRI_IS_SELLER.value": {
                  "$eq": true
                }
              },
              {
                "query.document.attributes.PRI_INTERN_ACCEPTED.value": {
                  "$ne": true
                }
              }
            ]
          },
          {
            "$and": [
              {
                "$or": [
                  {
                    "user.attributes.PRI_IS_EDU_PROVIDER_STAFF.value": {
                      "$eq": true
                    }
                  },
                  {
                    "user.attributes.PRI_IS_AGENT.value": {
                      "$eq": true
                    }
                  }
                ]
              },
              {
                "query.document.attributes.PRI_EDU_PROVIDER_ACCEPTED.value": {
                  "$ne": true
                }
              }
            ]
          }
        ]
      },
      "props": {
        "questionGroupCode": "{{navigation.offerId}}",
        "style": {
          "width": "100%",
          "marginTop": "40px"
        },
        "renderSubmitButton": {
          "component": "Button",
          "props": {
            "showSpinnerOnClick": true,
            "color": "green",
            "width": "100%"
          }
        }
      }
    }
  ],
  "query": [
    {
      "operator": "getBE",
      "id": "{{navigation.id}}",
      "as": "application"
    },
    {
      "operator": "scope",
      "path": "application",
      "scope": {
        "operator": "populateAttributes",
        "code": "{{code}}",
        "as": "attributes"
      }
    },
    {
      "operator": "scope",
      "path": "application",
      "scope": {
        "operator": "populateLinkValues",
        "withAttributes": true,
        "as": "linkValues"
      }
    },
    {
      "operator": "getLinks",
      "id": "application",
      "as": "application.links"
    },
    {
      "operator": "scope",
      "path": "application.links",
      "scope": {
        "operator": "getBE",
        "id": "{{link.targetCode}}"
      }
    },
    {
      "operator": "scope",
      "path": "application.links",
      "scope": {
        "operator": "find",
        "query": {
          "code": {
            "$exists": true
          }
        }
      }
    },
    {
      "operator": "scope",
      "path": "application.links",
      "as": "document",
      "scope": {
        "operator": "findOne",
        "query": {
          "$and": [{
              "code": {
                "$exists": true
              }
            },
            {
              "linkValue": {
                "$eq": "INT_AGMT"
              }
            }
          ]
        }
      }
    },
    {
      "operator": "scope",
      "path": "application.links",
      "scope": {
        "operator": "populateAttributes",
        "code": "{{code}}",
        "as": "attributes"
      }
    },
    {
      "operator": "clean",
      "fields": [
        "application", "document"
      ]
    }
  ]
}