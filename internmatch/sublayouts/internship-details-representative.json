{
    "Grid": {
        "rows": 1,
        "cols": [
            {
                "style": {
                    "flex": "0 0 50px",
                    "display": "flex",
                    "alignItems": "center",
                    "paddingRight": "5px",
                    "height": ""
                }
            },
            2,
            {
                "style": {
                    "flex": "0 0 50px",
                    "display": "flex",
                    "alignItems": "center",
                    "justifyContent": "flex-end",
                    "paddingRight": "5px"
                }
            }
        ],
        "style": {
            "boxShadow":"rgba(0, 0, 0, 0.12) 0px 1px 3px, rgba(0, 0, 0, 0.24) 0px 1px 2px",
            "borderRadius":"5px"
        },
        "children": [
            {
                "ProfileImageView": {
                    "position": [0,0],
                    "style": {
                        "width": "100%"
                    },
                    "src": "${BE.link.REPRESENTATIVE.PRI_IMAGE_URL}"
                }
            },
            {
                "Grid": {
                    "position": [0, 1],
                    "rows": 3,
                    "cols": 1,
                    "children": [
                        {
                            "span": {
                                "position": [0, 0],
                                "style": {
                                    "fontSize": "0.8em"
                                },
                                "children": "${BE.link.REPRESENTATIVE.PRI_NAME}"
                            }
                        },
                        {
                            "span": {
                                "position": [1, 0],
                                "style": {
                                    "fontSize": "0.75em"
                                },
                                "children": "${BE.link.REPRESENTATIVE.PRI_EMAIL}"
                            }
                        },
                        {
                            "span": {
                                "position": [2, 0],
                                "style": {
                                    "fontSize": "0.75em"
                                },
                                "children": "${BE.link.REPRESENTATIVE.PRI_MOBILE}"
                            }
                        }
                    ]
                }
            }

        ]
    }
}
  