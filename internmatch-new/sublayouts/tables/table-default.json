{
  "layout": {
    "title": "{{props.title}}",
    "appColor": "white",
    "backgroundColor": "white"
  },
  "children": [
    {
      "component": "Table",
      "dontInjectContextIntoProps": [
        "dispatchActionOnChange"
      ],
      "props": {
        "code": "{{props.code}}",
        "isSelectable": true,
        "selectFirstItemOnMount": true,
        "itemToSelectFirst": "_query.currentItem",
        "cellContext": {
          "rootCode": "{{props.rootCode}}",
          "userCode": "{{user.data.code}}"
        },
        "renderWrapper": {
          "component": "EventButton",
          "props": {
            "height": "20px",
            "fontWeight": "300",
            "fontSize": 10,
            "eventType": "TABLE_SELECT",
            "value": {
              "itemCode": "{{celld.code}}",
              "hint": "{{rootCode}}",
              "userCode": "{{userCode}}"
            },
            "dispatchActionOnClick": {
              "type": "ALIAS_ADD",
              "payload": {
                "alias": "CURRENT_NOTE",
                "value": "{{celld.code}}"
              }
            },
            "onPress": "_onPress"
          }
        },
        "dispatchActionOnChange": {
          "type": "ALIAS_ADD",
          "payload": {
            "alias": "CURRENT_NOTE",
            "value": "{{code}}"
          }
        },
        "columns": "_props.columns",
        "tableBackgroundColor": "#cccccc",
        "data": "_props.data",
        "height": "500px",
        "width": "500px",
        "itemsPerPage": 15,
        "totalItems": "_props.totalItems"
      }
    }
  ],
  "query": [
    {
      "operator": "getFromAlias",
      "alias": "CURRENT_NOTE",
      "as": "currentItem"
    }
  ]
}