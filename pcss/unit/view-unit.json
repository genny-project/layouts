{
  "Grid": {
    "rows": 1,
    "cols": [2, 1],
    "children": [
      {
        "div": {
          "position": [0, 0],
          "children": [
            {
              "div": {
                "style": {
                  "width": "100%",
                  "padding": "20px 50px 10px",
                  "position": "absolute"
                },
                "children": [
                  {
                    "GennyButton": {
                      "style": {
                        "width": "100px",
                        "height": "30px",
                        "position": "absolute"
                      },
                      "buttonStyle": {
                        "background": "PROJECT.PRI_COLOR",
                        "height": "30px",
                        "padding": "0"
                      },
                      "buttonCode": "BTN_BACK_BUTTON",
                      "hint": {
                        "itemCode": "BE.code"
                      },
                      "children": [
                        {
                          "IconSmall": {
                            "style": {
                              "fontSize": "28px",
                              "transform": "rotate(90deg)",
                              "color": "white"
                            },
                            "name": "arrow_drop_down"
                          }
                        },
                        {
                          "span": {
                            "style": {
                              "color": "white"
                            },
                            "children": "BACK"
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            },
            {
              "SublayoutLoader": {
                "position": [0, 0],
                "layoutCode": "property-details-header",
                "aliases": {
                  "BE": "BE.code",
                  "GROUP": "ROOT"
                }
              }
            }
          ]
        }
      },
      {
        "Grid": {
          "position": [0, 0],
          "style": {
            "color": "rgb(72,84,98)",
            "lineHeight": "1.8em"
          },
          "rows": [0.1, 1],
          "cols": [
            {
              "style": {
                "flexGrow": 1,
                "marginLeft": "10%"
              }
            }
          ],
          "children": [
            {
              "h4": {
                "position": [0, 0],
                "style": {
                  "borderBottom": "2px solid #485461",
                  "borderTop": "2px solid #485461"
                },
                "children": "Unit ID ${BE.PRI_UNIT_ID}"
              }
            },
            {
              "h4": {
                "position": [0, 0],
                "style": {
                  "borderBottom": "2px solid #485461",
                  "borderTop": "2px solid #485461"
                },
                "children": "Owner Details"
              }
            },
            {
              "p": {
                "position": [0,0],
                "style": {
                  "margin": "5px"
                },
                "children": "Name: ${BE.PRI_OWNER_FIRSTNAME} ${BE.PRI_OWNER_LASTNAME}"
              }
            },
            {
              "p": {
                "position": [0,0],
                "style": {
                  "margin": "5px"
                },
                "children": "Email: ${BE.PRI_EMAIL}"
              }
            }
          ]
        }
      }
    ]
  }
}
