{
  "Grid": {
    "rows": [
      {
        "style": {
          "flexGrow": 1,
          "margin": "10px"
        }
      }
    ],
    "cols": 2,
    "children": [
      {
        "div": {
          "style": {
            "display": "flex",
            "alignItems": "center",
            "height": "100%"
          },
          "children": [
            {
              "IconSmall": {
                "name": "location_on",
                "style": {
                  "marginRight": "10px",
                  "color": "PROJECT.PRI_COLOR"
                }
              }
            },
            {
              "GennyAddressLabel": {
                "root": "BE.code",
                "attributePrefix": "PRI_ADDRESS",
                "hideStreetAddress": false
              }
            }
          ]
        }
      },
      {
        "div": {
          "style": {
            "display": "flex",
            "alignItems": "center",
            "height": "100%"
          },
          "children": [
            {
              "IconSmall": {
                "name": "date_range",
                "style": {
                  "marginRight": "10px",
                  "color": "PROJECT.PRI_COLOR"
                }
              }
            },
            {
              "DateLabel": {
                "date": "BE.PRI_START_DATE",
                "format": "DD MM YYYY"
              }
            }
          ]
        }
      },
      {
        "div": {
          "style": {
            "display": "flex",
            "alignItems": "center",
            "height": "100%"
          },
          "children": [
            {
              "IconSmall": {
                "name": "schedule",
                "style": {
                  "marginRight": "10px",
                  "color": "PROJECT.PRI_COLOR"
                }
              }
            },
            {
              "span": {
                "style": {
                  "marginRight": "10px",
                  "color": "PROJECT.PRI_COLOR"
                },
                "children": "BE.LNK_INTERNSHIP_DURATION"
              }
            }
          ]
        }
      }
    ]
  }
}
