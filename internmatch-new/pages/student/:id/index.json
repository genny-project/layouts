{
  "layout": {
    "title": "Profile",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    },
    "sidebarRight": {
      "variant": "default",
      "onlyShowIf": {
        "$or": [
          {
            "user.attributes.PRI_IS_AGENT.value": {
              "$eq": true
            }
          },
          {
            "user.attributes.PRI_IS_EDU_PROVIDER_STAFF.value": {
              "$eq": true
            }
          },
          {
            "user.attributes.PRI_IS_BUYER.value": {
              "$eq": true
            }
          }
        ]
      }
    }
  },
  "children": [
    {
      "component": "Box",
      "props": {
        "flex": 1
      },
      "children": [
        {
          "component": "Sublayout",
          "props": {
            "layoutName": "user-profile",
            "user": "_query.student",
            "showBackButton": true
          }
        }
      ]
    }
  ],
  "query": [
    {
      "operator": "getBE",
      "id": "{{navigation.id}}",
      "as": "student"
    },
    {
      "operator": "scope",
      "path": "student",
      "scope": {
        "operator": "populateAttributes",
        "code": "{{code}}",
        "as": "attributes"
      }
    }
  ]
}

