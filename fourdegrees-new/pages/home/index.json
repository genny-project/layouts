{
  "layout": {
    "title": "Home",
    "appColor": "dark",
    "header": {
      "variant": "transparent"
    },
    "sidebar": {
      "variant": "default"
    }
  },
  "children": [
    {
      "component": "SafeAreaView",
      "props": {
        "forceInset": {
          "top": "never"
        },
        "style": {
          "flex": 1
        }
      },
      "children": [
        {
          "component": "Tabs",
          "props": {
            "bottomTabs": true,
            "tabBarBackground": "white",
            "iconColor": "#898f9f",
            "activeIconColor": "#202532",
            "iconSize": "lg",
            "labelColor": "#898f9f",
            "activeLabelColor": "#202532",
            "labelProps": {
              "minHeight": 20,
              "paddingY": 0,
              "size": "xs"
            },
            "tabs": [
              {
                "key": 0,
                "route": "Search",
                "title": "Search",
                "icon": "search"
              },
              {
                "key": 1,
                "route": "alerts",
                "title": "Alerts",
                "icon": "notifications"
              },
              {
                "key": 2,
                "route": "profile",
                "title": "Profile",
                "icon": "people"
              },
              {
                "key": 3,
                "route": "more",
                "title": "More",
                "icon": "settings"
              }
            ]
          },
          "children": [
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "search"
              }
            },
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "alerts"
              }
            },
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "profile",
                "userID": "{{user.data.code}}"
              }
            },
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "more"
              }
            }
          ]
        }
      ]
    }
  ]
}
