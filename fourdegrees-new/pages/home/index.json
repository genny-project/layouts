{
  "layout": {
    "title": "Home",
    "appColor": "dark",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    },
    "backgroundColor": "#f8f8f8"
  },
  "children": [
    {
      "component": "SafeAreaView",
      "props": {
        "forceInset": {
          "top": "never"
        },
        "style": {
          "flex": 1
        }
      },
      "children": [
        {
          "component": "Tabs",
          "props": {
            "bottomTabs": true,
            "tabBarBackground": "white",
            "iconColor": "#898f9f",
            "activeIconColor": "#202532",
            "iconSize": "lg",
            "labelColor": "#898f9f",
            "activeLabelColor": "#202532",
            "labelProps": {
              "minHeight": 20,
              "paddingY": 0,
              "size": "xs"
            },
            "flex": 1,
            "tabWrapperProps": {
              "paddingLeft": 20,
              "height": 50,
              "flexBasis": "auto"
            },
            "textColor": "#FFF",
            "tabBackground": "#41505d",
            "activeTabBackground": "#fff",
            "activeTabTextColor": "#000",
            "tabProps": {
              "paddingX": 20,
              "paddingY": 15,
              "minWidth": 180,
              "justifyContent": "center"
            },
            "tabTitleProps": {
              "size": "xs"
            },
            "activeTabTitleProps": {
              "bold": true
            },
            "inactiveTabProps": {
              "borderColor": "#233a4e",
              "borderRightWidth": 1,
              "borderStyle": "solid"
            },
            "childProps": {
              "overflow": "auto",
              "maxHeight": "calc(100vh - 65px - 50px)"
            },
            "overflowX": "auto",
            "backgroundColor": "#f6f6f6",
            "tabs": [
              {
                "key": 0,
                "route": "Search",
                "title": "Search",
                "icon": "search"
              },
              {
                "key": 1,
                "route": "alerts",
                "title": "Alerts",
                "icon": "notifications"
              },
              {
                "key": 2,
                "route": "chat",
                "title": "Chat",
                "icon": "inbox"
              },
              {
                "key": 3,
                "route": "profile",
                "title": "Profile",
                "icon": "people"
              },
              {
                "key": 4,
                "route": "more",
                "title": "More",
                "icon": "settings"
              }
            ]
          },
          "children": [
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "search"
              }
            },
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "alerts"
              }
            },
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "chat"
              }
            },
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "profile",
                "userID": "{{user.data.code}}"
              }
            },
            {
              "component": "Sublayout",
              "props": {
                "layoutName": "more"
              }
            }
          ]
        }
      ]
    }
  ]
}
