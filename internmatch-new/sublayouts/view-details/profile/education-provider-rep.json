{
  "layout": {
    "title": "Education Provider Rep Profile",
    "appColor": "white",
    "backgroundColor": "silver"
  },
  "children": [
    {
      "component": "Box",
      "props": {
        "flexDirection": "column",
        "backgroundColor": "rgb(255,255,255)"
      },
      "children": [
        {
          "component": "Sublayout",
          "props": {
            "layoutName": "view-details/header",
            "attributes": {
              "code": "{{attributeCode}}",
              "name": "{{props.fullName}}",
              "imageUrl": "{{props.imageUrl}}"
            },
            "universityName": "{{props.universityName}}",
            "buttonCode": "BTN_EDIT_EDU_PROVIDER_STAFF",
            "mainContent": {
              "component": "Box",
              "renderAsComponent": true,
              "props": {
                "flexDirection": "column",
                "marginTop": "20px"
              },
              "children": [
                {
                  "component": "Text",
                  "props": {
                    "color": "rgb(5, 88, 188)",
                    "size": "md",
                    "bold": false,
                    "text": "{{props.universityName}}"
                  },
                  "children": ""
                }
              ]
            }
          }
        }
      ]
    },
    {
      "component": "Box",
      "props": {
        "flexDirection": "column",
        "backgroundColor": "rgb(255,255,255)",
        "marginTop": "20px"
      },
      "children": [
        {
          "component": "Sublayout",
          "props": {
            "layoutName": "view-details/content",
            "title": "Contact",
            "content": [
              {
                "label": "Email:",
                "text": "{{props.email}}"
              },
              {
                "label": "Mobile:",
                "text": "{{props.attributes.PRI_MOBILE.value}}"
              }
            ]
          }
        }
      ]
    }
  ]
}
