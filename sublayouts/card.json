                        {
                            "div": {
                                "position": [0, 1],
                                "children": [
                                    {
                                        "span": {
                                            "style": {
                                                "fontSize": "0.75em"
                                            },
                                            "children": "$"
                                        }
                                    },
                                    {
                                        "span": {
                                            "style": {
                                                "fontSize": "0.75em"
                                            },
                                            "children": "BE.PRI_PRICE"
                                        }
                                    }    
                                ]
                            }
                        },
                        {
                            "div": {
                                "position": [0, 1],
                                "style": {
                                    "display": "flex",
                                    "flexDirection": "row"
                                },
                                "children": [
                                    {
                                        "span": {
                                            "style": {
                                                "fontSize": "0.75em"
                                            },
                                            "children": "BE.PRI_PICKUP_ADDRESS_SUBURB"
                                        }
                                    },
                                    {
                                        "span": {
                                            "style": {
                                                "fontSize": "0.75em",
                                                "marginRight": "5px"
                                            },
                                            "children": ","
                                        }
                                    },
                                    {
                                        "span": {
                                            "style": {
                                                "fontSize": "0.75em"
                                            },
                                            "children": "BE.PRI_PICKUP_ADDRESS_STATE"
                                        }
                                    },
                                    {
                                        "IconSmall": {
                                            "name": "arrow_right",
                                            "style": {
                                             "marginTop": "-5px"   
                                            }
                                        }
                                    },
                                    {
                                        "span": {
                                            "style": {
                                                "fontSize": "0.75em"
                                            },
                                            "children": "BE.PRI_DROPOFF_ADDRESS_SUBURB"
                                        }
                                    },
                                    {
                                        "span": {
                                            "style": {
                                                "fontSize": "0.75em",
                                                "marginRight": "5px"
                                            },
                                            "children": ","
