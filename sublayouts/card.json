                            }
                        },
                        {
                            "Status": {
                                "position": [0,2],   
                                "color": "success",
                                "style": {
                                    "height": "100%"
                                }
                            }
                        }
                    ]
                }
            },
            {
                "GennyDropdown" : {
                    "inline": true,
                    "isSlide": true,
                    "contentStyle": {
                        "backgroundColor": "white",
                        "padding": "0"
                    },
                    "header": [
                        {
                            "IconSmall": {
                                "name": "expand_more"
                            }
                        }
                    ],
                    "children": [
                        {
                            "Grid": {
                                "rows": 2,
                                "cols": [
                                    2,
                                    { 
                                        "style" : {
                                            "flexGrow": 4,
                                            "display": "flex",
                                            "alignItems": "center"
                                        }
