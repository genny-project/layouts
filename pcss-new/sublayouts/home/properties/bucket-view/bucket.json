{
  "children": [
    {
      "component": "Box",
      "props": {
        "flexDirection": "column",
        "alignItems": "center",
        "justifyContent": "center",
        "flex": 1,
        "padding": 20
      },
      "children": [
        {
          "component": "BucketView",
          "props": {
            "tabBarSize": "md",
            "backgroundColor": "#f6f6f6",
            "tabBarBackground": "#3f3f3f",
            "activeTabBackground": "#232323",
            "iconColor": "white",
            "iconSize": "md",
            "labelColor": "white",
            "labelProps": {
              "transform": "upperCase"
            },
            "overflowX": "auto",
            "overflowY": "auto",
            "width": "100%",
            "gutter": 20,
            "contentProps": {
              "minWidth": 300
            }
          },
          "repeat": "_query",
          "children": {
            "component": "List",
            "props": {
              "headerText": "{{repeater.name}}",
              "contentBackground": "#eaeaea",
              "overflowX": "auto",
              "headerProps": {
                "paddingX": 20,
                "paddingY": 15,
                "justifyContent": "space-between",
                "alignItems": "center",
                "backgroundColor": "#eaeaea",
                "position": "sticky",
                "top": 0
              },
              "headerTextProps": {
                "bold": true
              },
              "showCountInHeader": true,
              "contentBodyProps": {
                "paddingX": 10,
                "paddingY": 20
              },
              "emptyProps": {
                "padding": 10
              },
              "headerCountTextProps": {
                "bold": true,
                "size": "xs"
              },
              "emptyTextProps": {
                "size": "xs"
              },
              "scrollViewProps": {
                "horizontal": true
              }
            },
            "repeat": "_repeater.items",
            "children": {
              "component": "Touchable",
              "props": {
                "width": "100%",
                "backgroundColor": "#ffffff",
                "padding": 10,
                "paddingRight": 35,
                "key": "{{repeater.code}}",
                "position": "relative",
                "withFeedback": true,
                "showSpinnerOnClick": true,
                "value": {
                  "itemCode": "{{repeater.code}}"
                },
                "flexDirection": "column",
                "onPress": "_selection.onSelect",
                "dispatchActionOnClick": {
                  "type": "ALIAS_TOGGLE",
                  "payload": {
                    "alias": "CURRENT_NOTE",
                    "value": "{{repeater.code}}"
                  }
                }
              },
              "conditional": {
                "if": {
                  "repeater.code": {
                    "$exists": false
                  }
                },
                "then": {
                  "children": {
                    "component": "Box",
                    "props": {
                      "padding": 15,
                      "width": "100%",
                      "justifyContent": "center"
                    },
                    "children": [
                      {
                        "component": "ActivityIndicator",
                        "props": {
                          "size": "small"
                        }
                      }
                    ]
                  }
                }
              },
              "children": {
                "component": "Box",
                "onlyShowIf": {
                  "repeater.code": {
                    "$exists": true
                  }
                },
                "conditional": {
                  "if": {
                    "$or": []
                  },
                  "then": {
                    "opacity": 0.7
                  }
                },
                "props": {
                  "width": "100%",
                  "position": "static"
                },
                "children": [
                  {
                    "component": "Box",
                    "props": {
                      "position": "absolute",
                      "right": 0,
                      "top": 0,
                      "zIndex": 5
                    },
                    "children": [
                      {
                        "component": "Dropdown",
                        "props": {
                          "facingRight": true,
                          "menuButtonStyle": {
                            "backgroundColor": "transparent",
                            "border": 0,
                            "padding": 5,
                            "fontSize": 12,
                            "cursor": "pointer",
                            "color": "white"
                          },
                          "menuListStyle": {
                            "background": "white",
                            "borderWidth": 1,
                            "borderColor": "#bbbbbb",
                            "borderStyle": "solid"
                          },
                          "menuItemStyle": {
                            "textAlign": "center",
                            "padding": 5,
                            "color": "white",
                            "backgroundColor": "#5cb85c",
                            "marginTop": 5
                          }
                        },
                        "conditional": {
                          "if": {
                            "repeater.attributes.PRI_IS_DEACTIVATED.value": {
                              "$eq": true
                            }
                          },
                          "then": {
                            "items": [
                              {
                                "text": "Re-Activate",
                                "buttonCode": "BTN_ACTIVATE_APPLICATION",
                                "value": {
                                  "itemCode": "{{repeater.code}}",
                                  "hint": "{{parentRepeater.code}}"
                                }
                              }
                            ]
                          },
                          "else": {
                            "if": {
                              "repeater.attributes.PRI_GRP_COMPLETED_DATE.value": {
                                "$exists": true
                              }
                            },
                            "then": {
                              "items": [
                                {
                                  "text": "View Detail",
                                  "href": "/view-unit/{{repeater.code}}",
                                  "buttonCode": "BTN_VIEW_APPLICATION",
                                  "value": {
                                    "itemCode": "{{code}}",
                                    "hint": "BTN_VIEW_APPLICATION"
                                  }
                                }
                              ]
                            },
                            "else": {
                              "if": {
                                "repeater.parentCode": {
                                  "$eq": "GRP_IDENTIFIED"
                                }
                              },
                              "then": {
                                "items": [
                                  {
                                    "text": "View Detail",
                                    "href": "/view-unit/{{repeater.code}}",
                                    "buttonCode": "BTN_VIEW_APPLICATION",
                                    "value": {
                                      "itemCode": "{{code}}",
                                      "hint": "BTN_VIEW_APPLICATION"
                                    }
                                  },
                                  {
                                    "text": "Contact By Email",
                                    "buttonCode": "BTN_SEND_EMAIL",
                                    "value": {
                                      "itemCode": "{{repeater.code}}",
                                      "hint": "BTN_SEND_EMAIL"
                                    }
                                  },
                                  {
                                    "text": "Contact By Phone",
                                    "buttonCode": "BTN_CONTACTED_PHONE",
                                    "value": {
                                      "itemCode": "{{repeater.code}}",
                                      "hint": "BTN_CONTACTED_PHONE"
                                    }
                                  },
                                  {
                                    "text": "Contact By Letter",
                                    "buttonCode": "BTN_CONTACTED_LETTER",
                                    "value": {
                                      "itemCode": "{{repeater.code}}",
                                      "hint": "BTN_CONTACTED_LETTER"
                                    }
                                  },
                                  {
                                    "text": "Contact By Site Visit",
                                    "buttonCode": "BTN_CONTACTED_SITE_VISIT",
                                    "value": {
                                      "itemCode": "{{repeater.code}}",
                                      "hint": "BTN_CONTACTED_SITE_VISIT"
                                    }
                                  }
                                ]
                              },
                              "else": {
                                "if": {
                                  "repeater.parentCode": {
                                    "$eq": "GRP_CONTACTED"
                                  }
                                },
                                "then": {
                                  "items": [
                                    {
                                      "text": "View Detail",
                                      "href": "/view-unit/{{repeater.code}}",
                                      "buttonCode": "BTN_VIEW_APPLICATION",
                                      "value": {
                                        "itemCode": "{{code}}",
                                        "hint": "BTN_VIEW_APPLICATION"
                                      }
                                    },
                                    {
                                      "text": "Request Condition Report",
                                      "buttonCode": "BTN_REQUEST_INSPECTION",
                                      "value": {
                                        "itemCode": "{{repeater.code}}",
                                        "hint": "BTN_REQUEST_INSPECTION"
                                      }
                                    },
                                    {
                                      "text": "Decline Condition Report",
                                      "buttonCode": "BTN_REQUEST_DECLINE",
                                      "value": {
                                        "itemCode": "{{repeater.code}}",
                                        "hint": "BTN_REQUEST_DECLINE"
                                      }
                                    },
                                    {
                                      "text": "ReContact By Email",
                                      "buttonCode": "BTN_SEND_EMAIL_AGAIN",
                                      "value": {
                                        "itemCode": "{{repeater.code}}",
                                        "hint": "BTN_SEND_EMAIL_AGAIN"
                                      }
                                    },
                                    {
                                      "text": "ReContact By Phone",
                                      "buttonCode": "BTN_CONTACTED_PHONE_AGAIN",
                                      "value": {
                                        "itemCode": "{{repeater.code}}",
                                        "hint": "BTN_CONTACTED_PHONE_AGAIN"
                                      }
                                    },
                                    {
                                      "text": "ReContact By Letter",
                                      "buttonCode": "BTN_CONTACTED_LETTER_AGAIN",
                                      "value": {
                                        "itemCode": "{{repeater.code}}",
                                        "hint": "BTN_CONTACTED_LETTER_AGAIN"
                                      }
                                    },
                                    {
                                      "text": "ReContact By Site Visit",
                                      "buttonCode": "BTN_CONTACTED_SITE_VISIT_AGAIN",
                                      "value": {
                                        "itemCode": "{{repeater.code}}",
                                        "hint": "BTN_CONTACTED_SITE_VISIT_AGAIN"
                                      }
                                    }
                                  ]
                                },
                                "else": {
                                  "if": {
                                    "repeater.parentCode": {
                                      "$eq": "GRP_REQUESTED"
                                    }
                                  },
                                  "then": {
                                    "items": [
                                      {
                                        "text": "View Detail",
                                        "href": "/view-unit/{{repeater.code}}",
                                        "buttonCode": "BTN_VIEW_APPLICATION",
                                        "value": {
                                          "itemCode": "{{code}}",
                                          "hint": "BTN_VIEW_APPLICATION"
                                        }
                                      },
                                      {
                                        "text": "Book Inspection",
                                        "buttonCode": "BTN_REQUEST_INSPECTION_DATE",
                                        "value": {
                                          "itemCode": "{{repeater.code}}",
                                          "hint": "BTN_REQUEST_INSPECTION_DATE"
                                        }
                                      }
                                    ]
                                  },
                                  "else": {
                                    "if": {
                                      "repeater.parentCode": {
                                        "$eq": "GRP_INSPECTION"
                                      }
                                    },
                                    "then": {
                                      "items": [
                                        {
                                          "text": "View Detail",
                                          "href": "/view-unit/{{repeater.code}}",
                                          "buttonCode": "BTN_VIEW_APPLICATION",
                                          "value": {
                                            "itemCode": "{{code}}",
                                            "hint": "BTN_VIEW_APPLICATION"
                                          }
                                        },
                                        {
                                          "text": "Rebook Appointment",
                                          "buttonCode": "BTN_REBOOK_APPOINTMENT",
                                          "value": {
                                            "itemCode": "{{repeater.code}}",
                                            "hint": "BTN_REBOOK_APPOINTMENT"
                                          }
                                        },
                                        {
                                          "text": "Upload Building Condition Report",
                                          "buttonCode": "BTN_DEACTIVATE_APPLICATION",
                                          "value": {
                                            "itemCode": "{{repeater.code}}",
                                            "hint": "{{parentRepeater.code}}"
                                          }
                                        },
                                        {
                                          "text": "Inspection Completed",
                                          "buttonCode": "BTN_MOVE_PROPERTY_TO_SIGNED_OFF",
                                          "value": {
                                            "itemCode": "{{repeater.code}}",
                                            "hint": "BTN_MOVE_PROPERTY_TO_SIGNED_OFF"
                                          }
                                        }
                                      ]
                                    },
                                    "else": {
                                      "if": {
                                        "repeater.parentCode": {
                                          "$eq": "GRP_SIGNOFF"
                                        }
                                      },
                                      "then": {
                                        "items": [
                                          {
                                            "text": "View Detail",
                                            "href": "/view-unit/{{repeater.code}}",
                                            "buttonCode": "BTN_VIEW_APPLICATION",
                                            "value": {
                                              "itemCode": "{{code}}",
                                              "hint": "BTN_VIEW_APPLICATION"
                                            }
                                          },
                                          {
                                            "text": "Report Prepared",
                                            "buttonCode": "BTN_MARK_REPORT_AS_PREPARED",
                                            "value": {
                                              "itemCode": "{{repeater.code}}",
                                              "hint": "BTN_MARK_REPORT_AS_PREPARED"
                                            }
                                          },
                                          {
                                            "text": "Report Released",
                                            "buttonCode": "BTN_MARK_REPORT_AS_RELEASED",
                                            "value": {
                                              "itemCode": "{{repeater.code}}",
                                              "hint": "BTN_MARK_REPORT_AS_RELEASED"
                                            }
                                          },
                                          {
                                            "text": "Upload Condition Report",
                                            "buttonCode": "BTN_DEACTIVATE_APPLICATION",
                                            "value": {
                                              "itemCode": "{{repeater.code}}",
                                              "hint": "{{parentRepeater.code}}"
                                            }
                                          }
                                        ]
                                      },
                                      "else": {
                                        "if": {
                                          "repeater.attributes.PRI_LAST_CONTACT_DATETIME.value": {
                                            "$exists": true,
                                            "$ne": "undefined"
                                          }
                                        },
                                        "then": {
                                          "items": [
                                            {
                                              "text": "View Detail",
                                              "href": "/view-unit/{{repeater.code}}",
                                              "buttonCode": "BTN_VIEW_APPLICATION",
                                              "value": {
                                                "itemCode": "{{code}}",
                                                "hint": "BTN_VIEW_APPLICATION"
                                              }
                                            }
                                          ]
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        },
                        "children": [
                          {
                            "component": "Box",
                            "children": [
                              {
                                "component": "Icon",
                                "props": {
                                  "name": "more_horiz",
                                  "color": "#bbbbbb",
                                  "size": "sm"
                                }
                              }
                            ]
                          }
                        ]
                      },
                      {
                        "component": "Box",
                        "props": {
                          "flexDirection": "column"
                        },
                        "children": [
                          {
                            "component": "Box",
                            "conditional": [
                              {
                                "if": {
                                  "repeater.attributes.STA_GREEN.value": {
                                    "$in": "{{user.data.code}}"
                                  }
                                },
                                "then": {
                                  "backgroundColor": "green"
                                }
                              },
                              {
                                "if": {
                                  "repeater.attributes.STA_ORANGE.value": {
                                    "$in": "{{user.data.code}}"
                                  }
                                },
                                "then": {
                                  "backgroundColor": "orange"
                                }
                              },
                              {
                                "if": {
                                  "repeater.attributes.STA_RED.value": {
                                    "$in": "{{user.data.code}}"
                                  }
                                },
                                "then": {
                                  "backgroundColor": "red"
                                }
                              }
                            ],
                            "props": {
                              "size": "xs",
                              "height": 40,
                              "width": 10,
                              "borderRadius": 20,
                              "marginTop": 20,
                              "marginRight": 10
                            }
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "component": "Box",
                    "props": {
                      "margin": 10,
                      "marginRight": 20,
                      "flexShrink": 0,
                      "backgroundColor": "#ebeff3",
                      "alignItems": "center"
                    },
                    "children": [
                      {
                        "component": "Icon",
                        "conditional": [
                          {
                            "if": {
                              "$or": [
                                {
                                  "repeater.propertyType.name": {
                                    "$eq": "Residential (Multiple)"
                                  }
                                },
                                {
                                  "repeater.propertyType.name": {
                                    "$eq": "Commercial (Multiple)"
                                  }
                                }
                              ]
                            },
                            "then": {
                              "name": "location_city"
                            }
                          },
                          {
                            "if": {
                              "$or": [
                                {
                                  "repeater.propertyType.name": {
                                    "$eq": "Residential (Single)"
                                  }
                                },
                                {
                                  "repeater.propertyType.name": {
                                    "$eq": "Commercial (Single)"
                                  }
                                }
                              ]
                            },
                            "then": {
                              "name": "store"
                            }
                          },
                          {
                            "if": {
                              "repeater.propertyType.name": {
                                "$eq": "Parks"
                              }
                            },
                            "then": {
                              "name": "spa"
                            }
                          }
                        ],
                        "props": {
                          "color": "#e05b5c",
                          "size": "xl"
                        }
                      }
                    ]
                  },
                  {
                    "component": "Box",
                    "props": {
                      "flexDirection": "column",
                      "flexShrink": 1,
                      "width": "100%"
                    },
                    "children": [
                      {
                        "component": "Text",
                        "conditional": {
                          "if": {
                            "repeater.attributes.PRI_UNIT_ID.value": {
                              "$exists": true
                            }
                          },
                          "then": {
                            "text": "Unit: {{repeater.attributes.PRI_UNIT_ID.value}}"
                          }
                        },
                        "props": {
                          "size": "xs",
                          "bold": false
                        }
                      },
                      {
                        "component": "Text",
                        "conditional": {
                          "if": {
                            "repeater.attributes.PRI_ADDRESS_FULL.value": {
                              "$exists": true
                            }
                          },
                          "then": {
                            "text": "{{repeater.attributes.PRI_ADDRESS_FULL.value}}"
                          }
                        },
                        "props": {
                          "size": "xs",
                          "bold": false
                        }
                      },
                      {
                        "component": "Collapsible",
                        "props": {
                          "headerWrapperProps": {
                            "border": 0,
                            "padding": 5,
                            "fontSize": 12,
                            "cursor": "pointer",
                            "color": "white"
                          },
                          "headerIconProps": {
                            "background": "red",
                            "borderWidth": 1,
                            "borderColor": "#bbbbbb",
                            "borderStyle": "solid"
                          }
                        },
                        "children": [
                          {
                            "component": "Box",
                            "onlyShowIf": {
                              "repeater.parentCode": {
                                "$eq": "GRP_IDENTIFIED"
                              }
                            },
                            "props": {
                              "flexDirection": "column",
                              "width": "100%",
                              "justifyContent": "center"
                            },
                            "children": [
                              {
                                "component": "Text",
                                "props": {
                                  "flexDirection": "column",
                                  "justifyContent": "center",
                                  "color": "#000000",
                                  "text": "Owner"
                                },
                                "conditonal": {
                                  "if": {
                                    "$and": [
                                      {
                                        "repeater.attributes.PRI_OWNER_FIRSTNAME.value": {
                                          "$exists": true
                                        }
                                      },
                                      {
                                        "repeater.attributes.PRI_OWNER_LASTNAME.value": {
                                          "$exists": true
                                        }
                                      }
                                    ]
                                  },
                                  "then":{
                                    "text": "Owner: {{repeater.attributes.PRI_OWNER_FIRSTNAME.value}} {{repeater.attributes.PRI_OWNER_LASTNAME.value}}"
                                  }
                                }
                              },
                              {
                                "component": "Text",
                                "props": {
                                  "flexDirection": "column",
                                  "justifyContent": "center",
                                  "color": "#000000",
                                  "text": "Owner Email:"
                                },
                                "conditional": {
                                  "if": {
                                    "repeater.attributes.PRI_EMAIL.value": {
                                      "$exists": true
                                    }
                                  },
                                  "then": {
                                    "text": "Owner Email: {{repeater.attributes.PRI_EMAIL.value}}"
                                  }
                                }
                              },
                              {
                                "component": "Text",
                                "props": {
                                  "flexDirection": "column",
                                  "justifyContent": "center",
                                  "color": "#000000",
                                  "text": "Owner Mobile"
                                },
                                "conditional": {
                                  "if": {
                                    "repeater.attributes.PRI_MOBILE.value": {
                                      "$exists": true
                                    }
                                  },
                                  "then": {
                                    "text": "Owner Mobile: {{repeater.attributes.PRI_MOBILE.value}}"
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "component": "Box",
                            "onlyShowIf": {
                              "repeater.parentCode": {
                                "$eq": "GRP_CONTACTED"
                              }
                            },
                            "props": {
                              "flexDirection": "column",
                              "width": "100%",
                              "justifyContent": "center"
                            },
                            "children": [
                              {
                                "component": "Text",
                                "props": {
                                  "flexDirection": "column",
                                  "justifyContent": "center",
                                  "color": "#000000",
                                  "text": "Contact Method"
                                },
                                "conditonal": {
                                  "if": {
                                    "$and": [
                                      {
                                        "repeater.attributes.PRI_LAST_CONTACT_METHOD.value": {
                                          "$exists": true
                                        }
                                      }
                                    ]
                                  },
                                  "then":{
                                    "text": "Contact Type: {{repeater.attributes.PRI_LAST_CONTACT_METHOD.value}}"
                                  }
                                }
                              },
                              {
                                "component": "Text",
                                "props": {
                                  "flexDirection": "column",
                                  "justifyContent": "center",
                                  "color": "#000000",
                                  "text": "Last Contact Date"
                                },
                                "conditional": {
                                  "if": {
                                    "repeater.attributes.PRI_LAST_CONTACT_DATETIME.value": {
                                      "$exists": true
                                    }
                                  },
                                  "then": {
                                    "text": "Last Contact Date: {{repeater.attributes.PRI_LAST_CONTACT_DATETIME.value}}"
                                  }
                                }
                              },
                              {
                                "component": "Text",
                                "props": {
                                  "flexDirection": "column",
                                  "justifyContent": "center",
                                  "color": "#000000",
                                  "text": "No of Times Contacted:"
                                },
                                "conditional": {
                                  "if": {
                                    "repeater.attributes.PRI_NUMBER_OF_TIMES_CONTACTED.value": {
                                      "$exists": true
                                    }
                                  },
                                  "then": {
                                    "text": "No of Times Contacted:: {{repeater.attributes.PRI_NUMBER_OF_TIMES_CONTACTED.value}}"
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "component": "Box",
                            "onlyShowIf": {
                              "repeater.parentCode": {
                                "$eq": "GRP_INSPECTION"
                              }
                            },
                            "props": {
                              "flexDirection": "column",
                              "width": "100%",
                              "justifyContent": "center"
                            },
                            "children": [
                              {
                                "component": "Text",
                                "props": {
                                  "flexDirection": "column",
                                  "justifyContent": "center",
                                  "color": "#000000",
                                  "text": "Inspection Date:"
                                },
                                "conditonal": {
                                  "if": {
                                    "$and": [
                                      {
                                        "repeater.attributes.PRI_INSPECTION_DATE.value": {
                                          "$exists": true
                                        }
                                      }
                                    ]
                                  },
                                  "then":{
                                    "text": "Inspection Date: {{repeater.attributes.PRI_INSPECTION_DATE.value}}"
                                  }
                                }
                              }
                            ]
                          },

                          {
                            "component": "Box",
                            "props": {
                              "flexDirection": "column",
                              "width": "100%",
                              "justifyContent": "center"
                            },
                            "children": [
                              {
                                "component": "Text",
                                "props": {
                                  "flexDirection": "column",
                                  "justifyContent": "center",
                                  "color": "#000000"
                                },
                                "children": "{{repeater.attributes.PRI_COMPLETED_DATE.value}}",
                                "onlyShowIf": {
                                  "repeater.attributes.PRI_GRP_COMPLETED_DATE.value": {
                                    "$exists": true
                                  }
                                }
                              }
                            ]
                          },
