{
  "div": {
    "children": [
      {
        "Grid":{
            "cols": 2,
            "rows": [
              {
                "style": {
                  "flexGrow": 1
                }
              }, 
              {
                "style": {
                  "flexGrow": 1,
                  "marginTop": "20px"
                }
              },
              {
                "style": {
                  "flexGrow": 1,
                  "marginTop": "20px"
                }
              },
              {
                "style": {
                  "flexGrow": 1,
                  "marginTop": "20px"
                }
              },
              {
                "style": {
                  "flexGrow": 1,
                  "marginTop": "20px"
                }
              },
              {
                "style": {
                  "flexGrow": 1,
                  "marginTop": "20px"
                }
              }
            ],
            "style":{
                "height":"initial",
                "paddingBottom": "10px"
            },
            "children":[
                {
                    "span":{
                        "position":[0, 0],
                        "style":{
                            "fontSize":"1em",
                            "fontWeight":"bold"
                        },
                        "children":"JOB ID: ${BE.PRI_JOB_ID}"
                    }
                },
                {
                    "span":{
                        "position":[1, 0],
                        "style":{
                            "fontSize":"1em",
                            "fontWeight":"bold",
                            "marginTop": "20px"
                        },
                        "children":"PICKUP"
                    }
                },
                {
                    "SublayoutLoader" : {
                        "position":[1, 0],
                        "layoutCode": "address-pickup-display",
                        "aliases": {
                            "BE": "BE.code",
                            "GROUP": "ROOT"
                        }
                    }
                },
                {
                    "span":{
                        "position":[1, 1],
                        "style":{
                            "fontSize":"1em",
                            "fontWeight":"bold",
                            "marginTop": "10px"
                        },
                        "children":"DELIVERY"
                    }
                },
                {
                    "SublayoutLoader" : {
                        "position":[1, 1],
                        "layoutCode": "address-dropoff-display",
                        "aliases": {
                            "BE": "BE.code",
                            "GROUP": "ROOT"
                        }
                    }
                },
                {
                    "span":{
                        "position":[2, 0],
                        "style":{
                            "fontSize":"1em",
                            "fontWeight":"bold",
                            "marginTop": "10px"
                        },
                        "children":"AVAILABLE FOR PICKUP FROM"
                    }
                },
                {
                    "DateLabel":{
                        "position":[2, 0],
                        "format":"DD/MM/YY HH:mm",
                        "style":{
                            "fontSize":"0.8em"
                        },
                        "children":"BE.PRI_PICKUP_DATETIME"
                    }
                },
                {
                    "span":{
                        "position": [2, 1],
                        "style":{
                            "fontSize":"1em",
                            "fontWeight":"bold",
                            "marginTop": "10px"
                        },
                        "children":"DELIVERY REQUIRED BEFORE"
                    }
                },
                {
                  "p": {
                    "style": {
                      "margin": "0px"
                    },
                    "position": [2, 1],
                    "children": [
                    {
                        "Selector":{
                            "checkValues": "BE.PRI_DROPOFF_DATETIME",
                            "showIfNull": true,
                            "children":[
                                {
                                    "p":{
                                        "style": {
                                            "fontSize": "0.8em",
                                            "margin": "0px"
                                        },
                                        "children":"TBC"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "Selector":{
                            "checkValues": "BE.PRI_DROPOFF_DATETIME",
                            "showIfNotNull": true,
                            "children":[
                                {
                                    "DateLabel":{
                                        "format":"DD/MM/YY HH:mm",
                                        "style":{
                                            "fontSize":"0.8em"
                                        },
                                        "children":"BE.PRI_DROPOFF_DATETIME"
                                    }
                                }
                            ]
                        }
                    }
                    ]
                  }
                },
                {
                    "span":{
                        "position":[3, 0],
                        "style":{
                            "fontSize":"1em",
                            "fontWeight":"bold"
                        },
                        "children":"TOTAL DISTANCE"
                    }
                },
                {
                    "MeasurementLabel":{
                        "showDecimal": 2,
                        "position":[3, 0],
                        "style":{
                            "fontSize": "0.8em"
                        },
                        "children":"BE.PRI_TOTAL_DISTANCE_M"
                    }
                },
              {
                "div": {
                    "position":[4, 0],
                    "children":[
                        {
                            "span":{
                                "style": {
                                  "fontWeight":"bold",
                                  "fontSize": "0.8em"
                                },
                                "children":"LOAD INFORMATION"
                            }
                        }
                    ]
                }
              },
              {
                "div":{
                    "position":[4, 0],
                    "children":[
                        {
                            "DisplayHTML":{
                                "style":{
                                    "fontSize": "0.8em"
                                },
                                "children":"BE.link.LOAD.PRI_DESCRIPTION"
                            }
                        }
                    ]
                }
              },
              {
                "div":{
                    "position":[5,0],
                    "children":[
                        {
                            "span":{
                              "style": {
                                  "fontWeight":"bold",
                                  "fontSize": "0.8em"
                                },
                                "children":"LOAD DIMENSIONS"
                            }
                        }
                    ]
                }
              },
              {
                "Grid":{
                    "position":[5,0],
                    "cols":[3,3],
                    "rows":3,
                    "style":{
                        "height":"initial"
                    },
                    "children":[
                        {
                            "span":{
                                "position": [0,0],
                                "style": {
                                    "fontSize": "0.8em"
                                },
                                "children":"L: ${BE.link.LOAD.PRI_LOAD_LENGTH_MM}mm"
                            }
                        },
                        {
                            "span":{
                                "position": [0,1],
                                "style": {
                                    "fontSize": "0.8em"
                                },
                                "children":"W: ${BE.link.LOAD.PRI_LOAD_WIDTH_MM}mm"
                            }
                        },
                        {
                            "span":{
                                "position": [1,0],
                                "style": {
                                    "fontSize": "0.8em"
                                },
                                "children":"H: ${BE.link.LOAD.PRI_LOAD_HEIGHT_MM}mm"
                            }
                        },
                        {
                            "span":{
                                "position": [1,1],
                                "style": {
                                    "fontSize": "0.8em"
                                },
                                "children":"W: ${BE.link.LOAD.PRI_LOAD_WEIGHT_KG}kg"
                            }
                        }
                    ]
                }
              }
            ]
        }
      }
    ]
  }
}
