                                            "children": "BE.PRI_DROPOFF_DATETIME"
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "GennyList": {
                                "root": "BE.PRI_CODE",
                                "showLinks": true,
                                "itemsPerPage": 10,
                                "hideHeader": true,
                                "hideCount": true,
                                "style": {
                                    "padding": "0"   
                                }
                            }
                        },
                        {
                            "GennyButton": {
                                "buttonCode": "BTN_LOAD_SEE_MORE",
                                "value": {
                                    "itemCode": "BE.ITEM_CODE",
                                    "hint": "ROOT"
                                },
                                "position": [0,1],
                                "style": {
                                    "height": "30px",
                                    "marginTop": "10px"
                                },
                                "buttonStyle": {
                                    "backgroundColor": "#000"
                                },
                                "children": [
                                    {
                                        "span": {
                                            "style": {
                                                "fontSize": "0.8em"
                                            },
                                            "children": "View Full Job Details"
                                        }
                                    }
                                ]
                            } 
                        },
                        {
                            "Selector": {
                                "checkValues": "ROOT",
                                "showValues": "GRP_APPROVED",
                                "children": [
                                    {
                                        "ProgressBar": {
                                            "style": {
                                                "marginTop": "10px"   
                                            },
                                            "type": 2,
                                            "progressTotal": 100,
                                            "progressCurrent": 0
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "Selector": {
                                "checkValues": "ROOT",
                                "showValues": "GRP_IN_TRANSIT",
                                "children": [
                                    {
                                        "ProgressBar": {
                                            "style": {
                                                "marginTop": "10px"   
                                            },
                                            "type": 2,
                                            "progressTotal": 100,
                                            "progressCurrent": "BE.PRI_PROGRESS"
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "Selector": {
                                "checkValues": "ROOT",
                                "showValues": "GRP_DELIVERED",
                                "children": [
                                    {
                                        "ProgressBar": {
                                            "style": {
                                                "marginTop": "10px"   
                                            },
                                            "type": 2,
                                            "progressTotal": 100,
                                            "progressCurrent": 100
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    }
}
