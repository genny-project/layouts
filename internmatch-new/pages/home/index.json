{
  "layout": {
    "title": "Dashboard",
    "header": {
      "variant": "default"
    },
    "sidebar": {
      "variant": "default"
    }
  },
  "children": [
    {
      "component": "Sublayout",
      "onlyShowIf": {
        "user.attributes.PRI_IS_AGENT.value": {
          "$eq": true
        }
      },
      "props": {
        "layoutName": "home/agent"
      }
    },
    {
      "component": "Sublayout",
      "onlyShowIf": {
        "user.attributes.PRI_IS_EDU_PROVIDER_STAFF.value": {
          "$eq": true
        }
      },
      "props": {
        "layoutName": "home/agent"
      }
    },
    {
      "component": "Sublayout",
      "onlyShowIf": {
        "user.attributes.PRI_IS_SELLER.value": {
          "$eq": true
        }
      },
      "props": {
        "layoutName": "home/intern"
      }
    }
  ]
}
